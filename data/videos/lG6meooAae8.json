good.","type":"sentence","start_time":"00:03:15.820","end_time":"00:03:17.120","unapproved":1,"translation":{"text":"Có mùi thơm.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-39","text":"Two kilos of flour.","type":"sentence","start_time":"00:03:21.060","end_time":"00:03:23.320","unapproved":1,"translation":{"text":"Hai kg bột mì.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-40","text":"One kilo of salt.","type":"sentence","start_time":"00:03:26.800","end_time":"00:03:30.680","unapproved":1,"translation":{"text":"Một kg muối.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-41","text":"One kilo of sugar.","type":"sentence","start_time":"00:03:32.660","end_time":"00:03:34.800","unapproved":1,"translation":{"text":"Một kg đường.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-42","text":"And cocoa.","type":"sentence","start_time":"00:03:37.740","end_time":"00:03:40.320","unapproved":1,"translation":{"text":"Và ca cao.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}}],"view":"0","comment":"0","like":"0","dislike":"0","like_status":0,"is_news_a_minute":0,"slideshow":0,"id":"lG6meooAae8","url":"https://www.youtube.com/watch?v=fkEM8ybDDc4","title":"Pantry - English vocabulary","thumbnail":"https://i.ytimg.com/vi/fkEM8ybDDc4/hqdefault.jpg","mp4":0,"comment_id":null,"duration":"00:04:08","is_new":null}