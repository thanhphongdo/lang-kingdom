{"data":[{"id":"lG6meooAae8-1","text":"Hi. My name is Lukas. I am a teacher.","type":"sentence","start_time":"00:00:13.620","end_time":"00:00:17.220","unapproved":1,"translation":{"text":"Chào. Tôi tên là Lukas Tôi là một giáo viên.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-2","text":"Hi. I am Lukas a teacher as well.","type":"sentence","start_time":"00:00:17.720","end_time":"00:00:20.700","unapproved":1,"translation":{"text":"Chào. Tôi cũng là một giáo viên của Lukas.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-3","text":"Welcome to the pantry.","type":"sentence","start_time":"00:00:20.900","end_time":"00:00:23.300","unapproved":1,"translation":{"text":"Chào mừng đến với phòng đựng thức ăn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-4","text":"This is MonkeyEnglish.","type":"sentence","start_time":"00:00:23.900","end_time":"00:00:26.260","unapproved":1,"translation":{"text":"Đây là MonkeyEnglish.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-5","text":"And MonkeyEnglish is useful and simple.","type":"sentence","start_time":"00:00:26.260","end_time":"00:00:29.420","unapproved":1,"translation":{"text":"Và MonkeyEnglish rất hữu ích và đơn giản.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-6","text":"These are potatoes.","type":"sentence","start_time":"00:00:31.960","end_time":"00:00:34.040","unapproved":1,"translation":{"text":"Đây là khoai tây.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-7","text":"Wait a minute!","type":"sentence","start_time":"00:00:36.100","end_time":"00:00:37.100","unapproved":1,"translation":{"text":"Đợi tí!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-8","text":"Ok.","type":"sentence","start_time":"00:00:37.400","end_time":"00:00:37.900","unapproved":1,"translation":{"text":"Đồng ý.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-9","text":"Use the weight.","type":"sentence","start_time":"00:00:38.160","end_time":"00:00:40.140","unapproved":1,"translation":{"text":"Sử dụng trọng lượng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-10","text":"These are lemons.","type":"sentence","start_time":"00:00:46.800","end_time":"00:00:49.300","unapproved":1,"translation":{"text":"Đây là những quả chanh.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-11","text":"Onions.","type":"sentence","start_time":"00:00:54.020","end_time":"00:00:56.060","unapproved":1,"translation":{"text":"Hành.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-12","text":"This is ginger.","type":"sentence","start_time":"00:01:00.300","end_time":"00:01:02.280","unapproved":1,"translation":{"text":"Đây là gừng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-13","text":"Cabbage.","type":"sentence","start_time":"00:01:06.860","end_time":"00:01:08.920","unapproved":1,"translation":{"text":"Cải bắp.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-14","text":"It's going to be heavy.","type":"sentence","start_time":"00:01:14.300","end_time":"00:01:17.420","unapproved":1,"translation":{"text":"Nó sẽ nặng nề.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-15","text":"And these are carrots.","type":"sentence","start_time":"00:01:23.660","end_time":"00:01:27.260","unapproved":1,"translation":{"text":"Và đây là những củ cà rốt.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-16","text":"Pasta.","type":"sentence","start_time":"00:01:32.940","end_time":"00:01:36.120","unapproved":1,"translation":{"text":"Mỳ ống.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-17","text":"Rice.","type":"sentence","start_time":"00:01:41.200","end_time":"00:01:43.380","unapproved":1,"translation":{"text":"Cơm.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-18","text":"Olive oil.","type":"sentence","start_time":"00:01:46.420","end_time":"00:01:49.380","unapproved":1,"translation":{"text":"Dầu ô liu.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-19","text":"And...this is spice.","type":"sentence","start_time":"00:01:51.160","end_time":"00:01:54.980","unapproved":1,"translation":{"text":"Và ... đây là gia vị.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-20","text":"Can I smell?","type":"sentence","start_time":"00:01:57.420","end_time":"00:01:59.660","unapproved":1,"translation":{"text":"Tôi có thể ngửi không?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-21","text":"It smells interesting.","type":"sentence","start_time":"00:02:06.200","end_time":"00:02:08.340","unapproved":1,"translation":{"text":"Nó có mùi thú vị.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-22","text":"I know...it's spice.","type":"sentence","start_time":"00:02:08.620","end_time":"00:02:11.340","unapproved":1,"translation":{"text":"Tôi biết ... đó là gia vị.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-23","text":"Yes, it's spice.","type":"sentence","start_time":"00:02:12.980","end_time":"00:02:15.220","unapproved":1,"translation":{"text":"Vâng, đó là gia vị.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-24","text":"Good... Go on!","type":"sentence","start_time":"00:02:16.640","end_time":"00:02:18.840","unapproved":1,"translation":{"text":"Tốt ... Tiếp tục!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-25","text":"You definetely need to have preserved peppers.","type":"sentence","start_time":"00:02:20.220","end_time":"00:02:25.540","unapproved":1,"translation":{"text":"Bạn chắc chắn cần phải có ớt bảo quản.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-26","text":"Pickles.","type":"sentence","start_time":"00:02:30.960","end_time":"00:02:33.560","unapproved":1,"translation":{"text":"Dưa muối.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-27","text":"Tin of tuna.","type":"sentence","start_time":"00:02:36.600","end_time":"00:02:39.160","unapproved":1,"translation":{"text":"Tin của cá ngừ.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-28","text":"A jar of corn.","type":"sentence","start_time":"00:02:41.580","end_time":"00:02:45.000","unapproved":1,"translation":{"text":"Một lọ ngô.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-29","text":"A chocolate.","type":"sentence","start_time":"00:02:50.340","end_time":"00:02:51.580","unapproved":1,"translation":{"text":"Một sô cô la.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lG6meooAae8-30","text":"My favourite...","type":"sentence","start_time":"00:02:51.7