{"data":[{"id":"QbJ5nprQ0px-1","text":"Hi. This is MonkeyEnglish. My name is Lukas.","type":"sentence","start_time":"00:00:14.500","end_time":"00:00:17.460","unapproved":1,"translation":{"text":"Chào. Đây là MonkeyEnglish. Tôi tên là Lukas","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-2","text":"Hi. I am Lukas and we are teachers.","type":"sentence","start_time":"00:00:17.520","end_time":"00:00:19.600","unapproved":1,"translation":{"text":"Chào. Tôi là Lukas và chúng tôi là giáo viên.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-3","text":"We are travelling by train today.","type":"sentence","start_time":"00:00:21.120","end_time":"00:00:23.740","unapproved":1,"translation":{"text":"Chúng tôi đang đi du lịch bằng tàu hỏa ngày hôm nay.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-4","text":"This is railway station.","type":"sentence","start_time":"00:00:27.800","end_time":"00:00:29.940","unapproved":1,"translation":{"text":"Đây là ga đường sắt.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-5","text":"We will pick up our seat reservation tickets at this counter.","type":"sentence","start_time":"00:00:33.480","end_time":"00:00:39.660","unapproved":1,"translation":{"text":"Chúng tôi sẽ lấy vé đặt chỗ tại quầy này.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-6","text":"Departure board.","type":"sentence","start_time":"00:00:45.020","end_time":"00:00:47.720","unapproved":1,"translation":{"text":"Ban khởi hành.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-7","text":"This is a train.","type":"sentence","start_time":"00:00:48.820","end_time":"00:00:50.100","unapproved":1,"translation":{"text":"Đây là một chuyến tàu.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-8","text":"and this especially is a locomotive.","type":"sentence","start_time":"00:00:51.920","end_time":"00:00:54.880","unapproved":1,"translation":{"text":"và điều này đặc biệt là một đầu máy xe lửa.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-9","text":"Rails.","type":"sentence","start_time":"00:00:58.420","end_time":"00:01:00.160","unapproved":1,"translation":{"text":"Đường ray.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-10","text":"This is a platform.","type":"sentence","start_time":"00:01:01.920","end_time":"00:01:03.800","unapproved":1,"translation":{"text":"Đây là một nền tảng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-11","text":"People are running across the rails.","type":"sentence","start_time":"00:01:04.700","end_time":"00:01:07.960","unapproved":1,"translation":{"text":"Mọi người đang chạy trên đường ray.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-12","text":"Hello. You are a conductor?","type":"sentence","start_time":"00:01:10.420","end_time":"00:01:12.920","unapproved":1,"translation":{"text":"Xin chào. Bạn là một nhạc trưởng?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-13","text":"Yes.","type":"sentence","start_time":"00:01:13.140","end_time":"00:01:13.880","unapproved":1,"translation":{"text":"Đúng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-14","text":"Ok. Is this train to Jihlava?","type":"sentence","start_time":"00:01:14.100","end_time":"00:01:16.740","unapproved":1,"translation":{"text":"Đồng ý. Đây có phải là tàu đến Jihlava?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-15","text":"Yes.","type":"sentence","start_time":"00:01:17.080","end_time":"00:01:17.820","unapproved":1,"translation":{"text":"Đúng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-16","text":"Ok. Thank you very much.","type":"sentence","start_time":"00:01:18.180","end_time":"00:01:19.620","unapproved":1,"translation":{"text":"Đồng ý. Cảm ơn rât nhiều.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-17","text":"Get on!","type":"sentence","start_time":"00:01:23.240","end_time":"00:01:24.260","unapproved":1,"translation":{"text":"Leo lên!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-18","text":"We are getting on a passenger car.","type":"sentence","start_time":"00:01:24.640","end_time":"00:01:27.480","unapproved":1,"translation":{"text":"Chúng tôi đang lên một chiếc xe khách.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-19","text":"We are leaving the railway station.","type":"sentence","start_time":"00:01:47.760","end_time":"00:01:50.600","unapproved":1,"translation":{"text":"Chúng tôi đang rời ga đường sắt.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-20","text":"The train is long.","type":"sentence","start_time":"00:01:55.780","end_time":"00:01:57.300","unapproved":1,"translation":{"text":"Tàu dài.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-21","text":"There are twelve passenger cars altogether.","type":"sentence","start_time":"00:01:58.700","end_time":"00:02:03.320","unapproved":1,"translation":{"text":"Có mười hai chiếc xe chở khách hoàn toàn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-22","text":"The train is breaking.","type":"sentence","start_time":"00:02:13.260","end_time":"00:02:15.500","unapproved":1,"translation":{"text":"Tàu đang vỡ.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-23","text":"Freight cars.","type":"sentence","start_time":"00:02:21.960","end_time":"00:02:24.640","unapproved":1,"translation":{"text":"Xe chở hàng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-24","text":"Level crossing.","type":"sentence","start_time":"00:02:34.520","end_time":"00:02:36.740","unapproved":1,"translation":{"text":"Vượt cấp.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-25","text":"I am going back to my compartment.","type":"sentence","start_time":"00:02:39.500","end_time":"00:02:42.240","unapproved":1,"translation":{"text":"Tôi sẽ trở lại khoang của tôi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-26","text":"Another train station.","type":"sentence","start_time":"00:02:47.500","end_time":"00:02:49.100","unapproved":1,"translation":{"text":"Một nhà ga xe lửa khác.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-27","text":"Driver.","type":"sentence","start_time":"00:02:52.360","end_time":"00:02:53.900","unapproved":1,"translation":{"text":"Người lái xe.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QbJ5nprQ0px-28","text":"Platform.","type":"sentence","start_time":"00:03:03.180","end_time":"00:03:04.300","unapproved":1,"translation":{"text":"Nền tảng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}}],"view":"0","comment":"0","like