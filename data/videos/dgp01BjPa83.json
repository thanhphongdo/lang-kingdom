{"data":[{"id":"dgp01BjPa83-1","text":"This is a mask.","type":"sentence","start_time":"00:00:14.460","end_time":"00:00:16.100","unapproved":1,"translation":{"text":"Đây là một mặt nạ.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"dgp01BjPa83-2","text":"I am a robber.","type":"sentence","start_time":"00:00:16.880","end_time":"00:00:18.380","unapproved":1,"translation":{"text":"Tôi là một tên cướp.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"dgp01BjPa83-3","text":"I need some money!","type":"sentence","start_time":"00:00:20.020","end_time":"00:00:22.500","unapproved":1,"translation":{"text":"Tôi cần một ít tiền!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"dgp01BjPa83-4","text":"I am looking for some jewellery.","type":"sentence","start_time":"00:00:36.520","end_time":"00:00:39.320","unapproved":1,"translation":{"text":"Tôi đang tìm kiếm một số đồ trang sức.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"dgp01BjPa83-5","text":"Ah bingo!","type":"sentence","start_time":"00:00:40.140","end_time":"00:00:41.720","unapproved":1,"translation":{"text":"Ah chơi lô tô!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"dgp01BjPa83-6","text":"Do you know anyone who wants to sell jewellery to me?","type":"sentence","start_time":"00:01:05.100","end_time":"00:01:08.740","unapproved":1,"translation":{"text":"Bạn có biết ai muốn bán đồ trang sức cho tôi không?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"dgp01BjPa83-7","text":"I know someone.","type":"sentence","start_time":"00:01:09.100","end_time":"00:01:10.700","unapproved":1,"translation":{"text":"Tôi biết một vài người.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":