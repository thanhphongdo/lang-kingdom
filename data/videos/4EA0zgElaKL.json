{"data":[{"id":"4EA0zgElaKL-1","text":"We will teach you the bike vocab.","type":"sentence","start_time":"00:00:04.720","end_time":"00:00:08.200","unapproved":1,"translation":{"text":"Chúng tôi sẽ dạy bạn vocab xe đạp.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-2","text":"Hi. My name is Lukas.","type":"sentence","start_time":"00:00:14.220","end_time":"00:00:16.220","unapproved":1,"translation":{"text":"Chào. Tôi tên là Lukas","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-3","text":"Hi. My name is Lukas as well.","type":"sentence","start_time":"00:00:16.521","end_time":"00:00:18.361","unapproved":1,"translation":{"text":"Chào. Tôi tên là Lukas.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-4","text":"And we are teachers.","type":"sentence","start_time":"00:00:19.460","end_time":"00:00:20.820","unapproved":1,"translation":{"text":"Và chúng tôi là giáo viên.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-5","text":"We are riding the bike.","type":"sentence","start_time":"00:00:21.600","end_time":"00:00:24.220","unapproved":1,"translation":{"text":"Chúng tôi đang đi xe đạp.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-6","text":"We are riding uphill.","type":"sentence","start_time":"00:00:26.080","end_time":"00:00:28.460","unapproved":1,"translation":{"text":"Chúng tôi đang đi lên dốc.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-7","text":"These are handlebars.","type":"sentence","start_time":"00:00:34.340","end_time":"00:00:38.240","unapproved":1,"translation":{"text":"Đây là tay lái.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-8","text":"Levers,","type":"sentence","start_time":"00:00:42.060","end_time":"00:00:43.060","unapproved":1,"translation":{"text":"Đòn bẩy,","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-9","text":"And bowden cables.","type":"sentence","start_time":"00:00:45.100","end_time":"00:00:46.380","unapproved":1,"translation":{"text":"Và cáp Bowden.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-10","text":"Lukas, show me the bike.","type":"sentence","start_time":"00:00:53.420","end_time":"00:00:56.260","unapproved":1,"translation":{"text":"Lukas, cho tôi xem xe đạp.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-11","text":"Brakes.","type":"sentence","start_time":"00:01:01.360","end_time":"00:01:02.220","unapproved":1,"translation":{"text":"Phanh.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-12","text":"This is a front brake.","type":"sentence","start_time":"00:01:08.140","end_time":"00:01:11.200","unapproved":1,"translation":{"text":"Đây là một phanh trước.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-13","text":"And show me the rear brake.","type":"sentence","start_time":"00:01:13.100","end_time":"00:01:15.260","unapproved":1,"translation":{"text":"Và chỉ cho tôi phanh sau.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-14","text":"Yes, this was the rear brake.","type":"sentence","start_time":"00:01:21.160","end_time":"00:01:23.440","unapproved":1,"translation":{"text":"Vâng, đây là phanh sau.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-15","text":"And now show me the wheel.","type":"sentence","start_time":"00:01:25.040","end_time":"00:01:28.480","unapproved":1,"translation":{"text":"Và bây giờ chỉ cho tôi bánh xe.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-16","text":"This is the wheel.","type":"sentence","start_time":"00:01:31.040","end_time":"00:01:32.880","unapproved":1,"translation":{"text":"Đây là bánh xe.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-17","text":"Tire.","type":"sentence","start_time":"00:01:34.000","end_time":"00:01:34.760","unapproved":1,"translation":{"text":"Lốp xe.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-18","text":"It's the rubber part of the bike.","type":"sentence","start_time":"00:01:38.120","end_time":"00:01:41.040","unapproved":1,"translation":{"text":"Đó là phần cao su của xe đạp.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-19","text":"Rim.","type":"sentence","start_time":"00:01:42.320","end_time":"00:01:43.660","unapproved":1,"translation":{"text":"Vành.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-20","text":"Spoke.","type":"sentence","start_time":"00:01:48.620","end_time":"00:01:49.900","unapproved":1,"translation":{"text":"Đã nói.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-21","text":"Just one spoke.","type":"sentence","start_time":"00:01:52.280","end_time":"00:01:54.540","unapproved":1,"translation":{"text":"Chỉ cần một người nói.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-22","text":"This is a spoke.","type":"sentence","start_time":"00:01:54.960","end_time":"00:01:56.580","unapproved":1,"translation":{"text":"Đây là một lời nói.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-23","text":"This is a valve.","type":"sentence","start_time":"00:01:58.380","end_time":"00:02:00.500","unapproved":1,"translation":{"text":"Đây là một van.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-24","text":"What is the valve good for?","type":"sentence","start_time":"00:02:02.280","end_time":"00:02:04.700","unapproved":1,"translation":{"text":"Van tốt cho việc gì?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-25","text":"Valve - good for inflating.","type":"sentence","start_time":"00:02:06.120","end_time":"00:02:08.160","unapproved":1,"translation":{"text":"Van - tốt cho bơm hơi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-26","text":"Yes. That's right.","type":"sentence","start_time":"00:02:08.460","end_time":"00:02:09.780","unapproved":1,"translation":{"text":"Đúng. Đúng rồi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-27","text":"Show it to me!","type":"sentence","start_time":"00:02:10.700","end_time":"00:02:11.580","unapproved":1,"translation":{"text":"Cho tôi xem đi!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-28","text":"Ok.","type":"sentence","start_time":"00:02:11.900","end_time":"00:02:12.600","unapproved":1,"translation":{"text":"Đồng ý.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-29","text":"Tire pump.","type":"sentence","start_time":"00:02:17.160","end_time":"00:02:18.460","unapproved":1,"translation":{"text":"Bơm lốp.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-30","text":"What are you doing?","type":"sentence","start_time":"00:02:25.480","end_time":"00:02:27.320","unapproved":1,"translation":{"text":"Bạn đang làm gì đấy?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-31","text":"I am inflating.","type":"sentence","start_time":"00:02:28.040","end_time":"00:02:30.000","unapproved":1,"translation":{"text":"Tôi đang thổi phồng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-32","text":"Stop!","type":"sentence","start_time":"00:02:34.000","end_time":"00:02:34.940","unapproved":1,"translation":{"text":"Dừng lại!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-33","text":"I am pedalling.","type":"sentence","start_time":"00:02:37.780","end_time":"00:02:39.120","unapproved":1,"translation":{"text":"Tôi đang đạp.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-34","text":"I am riding zig zag.","type":"sentence","start_time":"00:02:43.160","end_time":"00:02:45.120","unapproved":1,"translation":{"text":"Tôi đang đi zig zag.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-35","text":"I am braking.","type":"sentence","start_time":"00:02:49.260","end_time":"00:02:50.460","unapproved":1,"translation":{"text":"Tôi đang phanh.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-36","text":"He is riding downstairs.","type":"sentence","start_time":"00:02:52.740","end_time":"00:02:55.120","unapproved":1,"translation":{"text":"Anh ấy đang đi xuống cầu thang.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-37","text":"This is a saddle.","type":"sentence","start_time":"00:03:08.140","end_time":"00:03:09.840","unapproved":1,"translation":{"text":"Đây là một cái yên ngựa.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-38","text":"This is a bottle cage","type":"sentence","start_time":"00:03:13.540","end_time":"00:03:15.880","unapproved":1,"translation":{"text":"Đây là một cái lồng chai","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-39","text":"and water bottle.","type":"sentence","start_time":"00:03:16.360","end_time":"00:03:18.280","unapproved":1,"translation":{"text":"và chai nước.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-40","text":"This is a pedal.","type":"sentence","start_time":"00:03:25.000","end_time":"00:03:26.500","unapproved":1,"translation":{"text":"Đây là một bàn đạp.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-41","text":"front derailleur","type":"sentence","start_time":"00:03:28.160","end_time":"00:03:29.540","unapproved":1,"translation":{"text":"trật bánh trước","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-42","text":"This is a chain.","type":"sentence","start_time":"00:03:37.000","end_time":"00:03:38.580","unapproved":1,"translation":{"text":"Đây là một chuỗi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-43","text":"and rear derailleur","type":"sentence","start_time":"00:03:40.440","end_time":"00:03:43.800","unapproved":1,"translation":{"text":"và trật bánh sau","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-44","text":"This is a frame.","type":"sentence","start_time":"00:03:51.100","end_time":"00:03:52.480","unapproved":1,"translation":{"text":"Đây là một khung.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zgElaKL-45