approved":1,"translation":{"text":"Ai quan tâm?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"9ev5ORWraOE-28","text":"Losers, anyway. Yeah.","type":"sentence","start_time":"00:01:06.486","end_time":"00:01:08.236","unapproved":1,"translation":{"text":"Dù sao thì thua cuộc. Vâng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"9ev5ORWraOE-29","text":"- [Observer] They were never gonna buy from you anyway.","type":"sentence","start_time":"00:01:09.640","end_time":"00:01:11.509","unapproved":1,"translation":{"text":"- [Người quan sát] Dù sao họ cũng sẽ không bao giờ mua hàng của bạn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"9ev5ORWraOE-30","text":"- Yeah, whatever.","type":"sentence","start_time":"00:01:11.509","end_time":"00:01:12.926","unapproved":1,"translation":{"text":"- Ừ thế nào cũng được.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"9ev5ORWraOE-31","text":"Not my target, not anything.","type":"sentence","start_time":"00:01:14.001","end_time":"00:01:15.191","unapproved":1,"translation":{"text":"Không phải mục tiêu của tôi, không phải bất cứ điều gì.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"9ev5ORWraOE-32","text":"I don't care.","type":"sentence","start_time":"00:01:15.191","end_time":"00:01:16.165","unapproved":1,"translation":{"text":"Tôi không quan tâm.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"9ev5ORWraOE-33","text":"I just don't care.","type":"sentence","start_time":"00:01:16.165","end_time":"00:01:17.054","unapproved":1,"translation":{"text":"Tôi không quan tâm.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"9ev5ORWraOE-34","text":"Who cares?","type":"sentence","start_time":"00:01:1