{"data":[{"id":"zkp602QqmnQ-1","text":"We will teach you the football match vocab.","type":"sentence","start_time":"00:00:03.580","end_time":"00:00:08.000","unapproved":1,"translation":{"text":"Chúng tôi sẽ dạy bạn vocab trận bóng đá.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"zkp602QqmnQ-2","text":"Hi. I am Lukas, a teacher.","type":"sentence","start_time":"00:00:14.180","end_time":"00:00:16.680","unapproved":1,"translation":{"text":"Chào. Tôi là Lukas, một giáo viên.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"zkp602QqmnQ-3","text":"This is MonkeyEnglish.","type":"sentence","start_time":"00:00:17.020","end_time":"00:00:18.440","unapproved":1,"translation":{"text":"Đây là MonkeyEnglish.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"zkp602QqmnQ-4","text":"MonkeyEnglish is useful and simple.","type":"sentence","start_time":"00:00:18.760","end_time":"00:00:21.380","unapproved":1,"translation":{"text":"MonkeyEnglish rất hữu ích và đơn giản.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"zkp602QqmnQ-5","text":"Let's watch a football match.","type":"sentence","start_time":"00:00:21.760","end_time":"00:00:24.460","unapproved":1,"translation":{"text":"Hãy cùng xem một trận bóng đá.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"zkp602QqmnQ-6","text":"This is a football team.","type":"sentence","start_time":"00:00:25.860","end_time":"00:00:28.820","unapproved":1,"translation":{"text":"Đây là một đội bóng đá.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"zkp602QqmnQ-7","text":"This is a coach.","type":"sentence","start_time":"00:00:30.280","end_time":"00:00:31.980","unapproved":1,"translation":{"text":"Đây là một huấn luyện viên.","google_translate":1},"record