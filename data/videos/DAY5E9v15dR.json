ng":0}},{"id":"DAY5E9v15dR-14","text":"♪ I let her steal into my melancholy heart ♪","type":"sentence","start_time":"00:00:33.566","end_time":"00:00:38.233 align:start size:91% position:9%","unapproved":1,"translation":{"text":"Tôi để cô ấy đánh cắp trái tim u sầu của mình","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DAY5E9v15dR-15","text":"♪ It's more than I can bear ♪","type":"sentence","start_time":"00:00:38.266","end_time":"00:00:41.533","unapproved":1,"translation":{"text":"Nó nhiều hơn tôi có thể chịu được","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DAY5E9v15dR-16","text":"♪ Now I know she'll never leave me ♪","type":"sentence","start_time":"00:00:44.566","end_time":"00:00:49.733 align:start size:88% position:13%","unapproved":1,"translation":{"text":"Bây giờ tôi biết cô ấy sẽ không bao giờ rời xa tôi","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DAY5E9v15dR-17","text":"♪ Even as she runs away ♪","type":"sentence","start_time":"00:00:49.766","end_time":"00:00:54.600","unapproved":1,"translation":{"text":"Ngay cả khi cô ấy chạy trốn","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DAY5E9v15dR-18","text":"♪ She will still torment me ♪","type":"sentence","start_time":"00:00:54.633","end_time":"00:00:57.033","unapproved":1,"translation":{"text":"Cô ấy vẫn sẽ hành hạ tôi","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DAY5E9v15dR-19","text":"♪ Calm me, hurt me ♪","type":"sentence","start_time":"00:00:57.066","end_time":"00:00:59.266","unapproved":1,"translation":{"text":"Bình tĩnh, làm tổn thương tôi","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DAY5E9v15dR-20","text":"♪ Move me, come what may ♪","type":"sentence","start_time":"00:00:59.300","end_time":"0