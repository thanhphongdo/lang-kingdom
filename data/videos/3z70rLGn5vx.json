{"data":[{"id":"3z70rLGn5vx-1","text":"My name is Olivia","type":"sentence","start_time":"00:00:00.820","end_time":"00:00:03.229","unapproved":1,"translation":{"text":"Tôi tên là Olivia","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"3z70rLGn5vx-2","text":"I like songs about animals,","type":"sentence","start_time":"00:00:03.229","end_time":"00:00:05.869","unapproved":1,"translation":{"text":"Tôi thích những bài hát về động vật,","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"3z70rLGn5vx-3","text":"Colors,","type":"sentence","start_time":"00:00:06.200","end_time":"00:00:07.109","unapproved":1,"translation":{"text":"Màu sắc,","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"3z70rLGn5vx-4","text":"And shapes.","type":"sentence","start_time":"00:00:07.560","end_time":"00:00:09.390","unapproved":1,"translation":{"text":"Và hình dạng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"3z70rLGn5vx-5","text":"Songs that help me learn my ABCs.","type":"sentence","start_time":"00:00:09.940","end_time":"00:00:12.520","unapproved":1,"translation":{"text":"Bài hát giúp tôi học ABCs của tôi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"3z70rLGn5vx-6","text":"And songs that help me get ready.","type":"sentence","start_time":"00:00:12.880","end_time":"00:00:14.920","unapproved":1,"translation":{"text":"Và những bài hát giúp tôi sẵn sàng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"3z70rLGn5vx-7","text":"Even songs for rainy days.","type":"sentence","start_time":"00:00:15.200","end_time":"00:00:17.760","unapproved":1,"translation":{"text":"Ngay cả những bài hát cho những ngày mưa.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"3z70rLGn5vx-8","text":"But silly songs are my favorite songs.","type":"sentence","start_time":"00:00:17.760","end_time":"00:00:20.880","unapproved":1,"translation":{"text":"Nhưng những bài hát ngớ ngẩn là những bài hát yêu thích của tôi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"3z70rLGn5vx-9","text":"Sometimes I just want to get up and DANCE!","type":"sentence","start_time":"00:00:21.220","end_time":"00:00:23.880","unapproved":1,"translation":{"text":"Đôi khi tôi chỉ muốn thức dậy và DANCE!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"3z70rLGn5vx-10","text":"Or listen to a song before I go to bed.","type":"sentence","start_time":"00:00:23.980","end_time":"00:00:27.460","unapproved":1,"translation":{"text":"Hoặc nghe một bài hát trước khi tôi đi ngủ.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"3z70rLGn5vx-11","text":"What kind of songs do you like?","type":"sentence","start_time":"00:00:27.779","end_time":"00:00:29.939","unapproved":1,"translation":{"text":"Bạn thích loại bài hát nào?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}}],"view":"0","comment":"0","like":"0","dislike":"0","like_status":0,"is_news_a_minute":0,"slideshow":0,"id":"3z70rLGn5vx","url":"https://www.youtube.com/watch?v=FMj4fyy9xrc","title":"Welcome to Super Simple Songs!","thumbnail":"https://i.ytimg.com/vi/FMj4fyy9xrc/maxresdefault.jpg","mp4":0,"comment_id":null,"duration":"00:00:30","is_new":null}