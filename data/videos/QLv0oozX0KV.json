{"data":[{"id":"QLv0oozX0KV-1","text":"Can you name the flower species?","type":"sentence","start_time":"00:00:12.020","end_time":"00:00:15.440","unapproved":1,"translation":{"text":"Bạn có thể đặt tên cho các loài hoa?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-2","text":"And the flower parts?","type":"sentence","start_time":"00:00:17.620","end_time":"00:00:19.840","unapproved":1,"translation":{"text":"Còn phần hoa?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-3","text":"We can help you!","type":"sentence","start_time":"00:00:20.140","end_time":"00:00:21.700","unapproved":1,"translation":{"text":"Chúng tôi có thể giúp bạn!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-4","text":"Let's surprise our sweethearts!","type":"sentence","start_time":"00:00:26.800","end_time":"00:00:29.460","unapproved":1,"translation":{"text":"Hãy làm bất ngờ người yêu của chúng tôi!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-5","text":"Good idea!","type":"sentence","start_time":"00:00:29.820","end_time":"00:00:31.080","unapproved":1,"translation":{"text":"Ý tưởng tốt!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-6","text":"I am going to buy a bunch.","type":"sentence","start_time":"00:00:31.800","end_time":"00:00:34.320","unapproved":1,"translation":{"text":"Tôi sẽ mua một bó.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-7","text":"I am going to pick the flowers for my darling.","type":"sentence","start_time":"00:00:34.680","end_time":"00:00:39.460","unapproved":1,"translation":{"text":"Tôi sẽ đi hái hoa cho người tôi yêu.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-8","text":"Hello!","type":"sentence","start_time":"00:00:41.400","end_time":"00:00:42.800","unapproved":1,"translation":{"text":"Xin chào!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-9","text":"May I have a bunch?","type":"sentence","start_time":"00:00:44.500","end_time":"00:00:46.940","unapproved":1,"translation":{"text":"Tôi có thể có một bó?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-10","text":"For my sweetheart.","type":"sentence","start_time":"00:00:47.100","end_time":"00:00:48.700","unapproved":1,"translation":{"text":"Dành cho người yêu của tôi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-11","text":"A rose.","type":"sentence","start_time":"00:00:51.260","end_time":"00:00:52.540","unapproved":1,"translation":{"text":"Một bông hồng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-12","text":"Which one? The orange one.","type":"sentence","start_time":"00:00:53.720","end_time":"00:00:56.760","unapproved":1,"translation":{"text":"Cái nào? Một quả cam.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-13","text":"This is a rose.","type":"sentence","start_time":"00:00:59.340","end_time":"00:01:01.380","unapproved":1,"translation":{"text":"Đây là một bông hồng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-14","text":"The rose has thorns.","type":"sentence","start_time":"00:01:02.340","end_time":"00:01:06.420","unapproved":1,"translation":{"text":"Hoa hồng có gai.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-15","text":"A tulip.","type":"sentence","start_time":"00:01:08.980","end_time":"00:01:10.600","unapproved":1,"translation":{"text":"Một bông hoa tulip.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-16","text":"The yellow one.","type":"sentence","start_time":"00:01:12.240","end_time":"00:01:14.380","unapproved":1,"translation":{"text":"Cái màu vàng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-17","text":"She will love it!","type":"sentence","start_time":"00:01:18.220","end_time":"00:01:19.520","unapproved":1,"translation":{"text":"Cô ấy sẽ thích nó!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-18","text":"A tulip.","type":"sentence","start_time":"00:01:19.680","end_time":"00:01:20.920","unapproved":1,"translation":{"text":"Một bông hoa tulip.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-19","text":"A carnation.","type":"sentence","start_time":"00:01:23.480","end_time":"00:01:25.320","unapproved":1,"translation":{"text":"Một bông hoa cẩm chướng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-20","text":"The white is ok?","type":"sentence","start_time":"00:01:29.520","end_time":"00:01:30.520","unapproved":1,"translation":{"text":"Màu trắng là ok?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-21","text":"Yes. The white one.","type":"sentence","start_time":"00:01:30.540","end_time":"00:01:32.300","unapproved":1,"translation":{"text":"Đúng. Một màu trắng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-22","text":"A carnation.","type":"sentence","start_time":"00:01:35.280","end_time":"00:01:37.440","unapproved":1,"translation":{"text":"Một bông hoa cẩm chướng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-23","text":"And a sunflower.","type":"sentence","start_time":"00:01:39.280","end_time":"00:01:41.560","unapproved":1,"translation":{"text":"Và một bông hoa hướng dương.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-24","text":"Thank you very much.","type":"sentence","start_time":"00:02:04.060","end_time":"00:02:05.420","unapproved":1,"translation":{"text":"Cảm ơn rât nhiều.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-25","text":"This is a bunch or bouquet.","type":"sentence","start_time":"00:02:06.360","end_time":"00:02:10.520","unapproved":1,"translation":{"text":"Đây là một bó hoặc bó hoa.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-26","text":"Bye!","type":"sentence","start_time":"00:02:13.960","end_time":"00:02:14.740","unapproved":1,"translation":{"text":"Tạm biệt!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-27","text":"I am going to pick some flowers for my darling.","type":"sentence","start_time":"00:02:15.160","end_time":"00:02:21.240","unapproved":1,"translation":{"text":"Tôi sẽ đi hái một vài bông hoa cho người tôi yêu.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-28","text":"It's a marguerite.","type":"sentence","start_time":"00:02:28.020","end_time":"00:02:30.540","unapproved":1,"translation":{"text":"Đó là một loại bơ.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-29","text":"Nice. We can name the parts of the flower.","type":"sentence","start_time":"00:02:31.340","end_time":"00:02:36.060","unapproved":1,"translation":{"text":"Đẹp. Chúng ta có thể đặt tên cho các bộ phận của hoa.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-30","text":"This is a flower.","type":"sentence","start_time":"00:02:38.400","end_time":"00:02:40.560","unapproved":1,"translation":{"text":"Đây là một bông hoa.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-31","text":"White flower with a yellow center.","type":"sentence","start_time":"00:02:41.300","end_time":"00:02:44.360","unapproved":1,"translation":{"text":"Hoa màu trắng với một trung tâm màu vàng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-32","text":"This is a stem.","type":"sentence","start_time":"00:02:45.180","end_time":"00:02:48.420","unapproved":1,"translation":{"text":"Đây là một thân cây.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-33","text":"And these are leaves.","type":"sentence","start_time":"00:02:51.900","end_time":"00:02:55.520","unapproved":1,"translation":{"text":"Và đây là những chiếc lá.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-34","text":"And these are rootlets.","type":"sentence","start_time":"00:02:58.400","end_time":"00:03:02.040","unapproved":1,"translation":{"text":"Và đây là những rễ con.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-35","text":"It's a dandelion.","type":"sentence","start_time":"00:03:07.040","end_time":"00:03:08.840","unapproved":1,"translation":{"text":"Đó là một cây bồ công anh.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-36","text":"Yellow, white... What do you think?","type":"sentence","start_time":"00:03:18.280","end_time":"00:03:21.980","unapproved":1,"translation":{"text":"Vàng, trắng ... Bạn nghĩ gì?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-37","text":"I am missing something... Red!","type":"sentence","start_time":"00:03:22.780","end_time":"00:03:25.500","unapproved":1,"translation":{"text":"Tôi đang thiếu thứ gì đó ... Màu đỏ!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-38","text":"Yes!","type":"sentence","start_time":"00:03:25.700","end_time":"00:03:26.760","unapproved":1,"translation":{"text":"Đúng!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-39","text":"There is a poppy.","type":"sentence","start_time":"00:03:26.880","end_time":"00:03:28.820","unapproved":1,"translation":{"text":"Có một cây anh túc.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-40","text":"This is a poppy.","type":"sentence","start_time":"00:03:34.600","end_time":"00:03:37.420","unapproved":1,"translation":{"text":"Đây là một cây anh túc.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-41","text":"Sorry!","type":"sentence","start_time":"00:03:43.820","end_time":"00:03:44.500","unapproved":1,"translation":{"text":"Lấy làm tiếc!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-42","text":"This is a daffodil.","type":"sentence","start_time":"00:03:51.260","end_time":"00:03:54.260","unapproved":1,"translation":{"text":"Đây là một bông thủy tiên.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-43","text":"This is a private property.","type":"sentence","start_time":"00:03:57.240","end_time":"00:03:59.780","unapproved":1,"translation":{"text":"Đây là một tài sản tư nhân.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-44","text":"You can't pick the daffodil.","type":"sentence","start_time":"00:04:00.540","end_time":"00:04:03.420","unapproved":1,"translation":{"text":"Bạn không thể chọn hoa thủy tiên.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-45","text":"Ok.","type":"sentence","start_time":"00:04:04.320","end_time":"00:04:05.420","unapproved":1,"translation":{"text":"Đồng ý.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-46","text":"You are right.","type":"sentence","start_time":"00:04:06.300","end_time":"00:04:07.820","unapproved":1,"translation":{"text":"Bạn đúng rồi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-47","text":"It's not a good idea.","type":"sentence","start_time":"00:04:07.860","end_time":"00:04:09.600","unapproved":1,"translation":{"text":"Nó không phải là một ý tưởng tốt.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-48","text":"And a daisy.","type":"sentence","start_time":"00:04:11.220","end_time":"00:04:13.520","unapproved":1,"translation":{"text":"Và một bông cúc.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-49","text":"A daisy.","type":"sentence","start_time":"00:04:16.520","end_time":"00:04:17.820","unapproved":1,"translation":{"text":"Một bông cúc.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-50","text":"It's small but nice.","type":"sentence","start_time":"00:04:18.220","end_time":"00:04:20.580","unapproved":1,"translation":{"text":"Nó nhỏ nhưng tốt.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0oozX0KV-51","text":"Does she love me or does she not?","type":"sentence","start_time":"00:04:29.400",