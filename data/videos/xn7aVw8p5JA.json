{"data":[{"id":"xn7aVw8p5JA-1","text":"Do you want to become an all-round sportsman?","type":"sentence","start_time":"00:00:11.560","end_time":"00:00:15.320","unapproved":1,"translation":{"text":"Bạn có muốn trở thành một vận động viên toàn diện?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"xn7aVw8p5JA-2","text":"Join our workout!","type":"sentence","start_time":"00:00:16.080","end_time":"00:00:21.080","unapproved":1,"translation":{"text":"Tham gia tập luyện của chúng tôi!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"xn7aVw8p5JA-3","text":"This is a skipping rope.","type":"sentence","start_time":"00:00:21.220","end_time":"00:00:25.800","unapproved":1,"translation":{"text":"Đây là một sợi dây bỏ qua.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"xn7aVw8p5JA-4","text":"I am skipping.","type":"sentence","start_time":"00:00:29.680","end_time":"00:00:32.540","unapproved":1,"translation":{"text":"Tôi đang bỏ qua.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"xn7aVw8p5JA-5","text":"Lift weights!","type":"sentence","start_time":"00:00:35.600","end_time":"00:00:39.020","unapproved":1,"translation":{"text":"Cử tạ!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"xn7aVw8p5JA-6","text":"Lukas is lifting weights.","type":"sentence","start_time":"00:00:50.820","end_time":"00:00:54.640","unapproved":1,"translation":{"text":"Lukas đang nâng tạ.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"xn7aVw8p5JA-7","text":"Kneel!","type":"sentence","start_time":"00:00:57.720","end_time":"00:00:59.820","unapproved":1,"translation":{"text":"Quỳ xuống!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"xn7aVw8p5JA-8","text":"on the mat."