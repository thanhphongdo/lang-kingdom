slate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"xn7aVw8p5JA-23","text":"Make crunches!","type":"sentence","start_time":"00:02:19.180","end_time":"00:02:21.940","unapproved":1,"translation":{"text":"Làm giòn!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"xn7aVw8p5JA-24","text":"Slowly, slowly!","type":"sentence","start_time":"00:02:25.480","end_time":"00:02:27.380","unapproved":1,"translation":{"text":"Từ từ từ từ!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"xn7aVw8p5JA-25","text":"Slalom the clubs!","type":"sentence","start_time":"00:02:38.300","end_time":"00:02:41.120","unapproved":1,"translation":{"text":"Slalom các câu lạc bộ!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"xn7aVw8p5JA-26","text":"Shoot the basket!","type":"sentence","start_time":"00:02:48.020","end_time":"00:02:50.700","unapproved":1,"translation":{"text":"Bắn rổ!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"xn7aVw8p5JA-27","text":"These are wall bars.","type":"sentence","start_time":"00:02:55.460","end_time":"00:02:58.460","unapproved":1,"translation":{"text":"Đây là những thanh treo tường.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"xn7aVw8p5JA-28","text":"Go on with your stomach workout!","type":"sentence","start_time":"00:03:03.420","end_time":"00:03:08.180","unapproved":1,"translation":{"text":"Tiếp tục với tập luyện dạ dày của bạn!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"xn7aVw8p5JA-29","text":"Nice!","type":"sentence","start_time":"00:03:16.860","end_time":"00:03:17.760","unapproved":1,"translation":{"text":"Đẹp!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"xn7aVw8p5JA-30","tex