{"data":[{"id":"ZGEraBWj53j-1","text":"I’d like a number 1, please.","type":"sentence","start_time":"00:00:07.500","end_time":"00:00:10.260","unapproved":1,"translation":{"text":"Tôi thích như số 1, làm ơn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZGEraBWj53j-2","text":"I’d like a number [the meal number], please.","type":"sentence","start_time":"00:00:10.260","end_time":"00:00:15.240","unapproved":1,"translation":{"text":"Tôi thích một số [số bữa ăn], xin vui lòng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZGEraBWj53j-3","text":"I’d like a number 1, please.","type":"sentence","start_time":"00:00:15.780","end_time":"00:00:18.400","unapproved":1,"translation":{"text":"Tôi thích như số 1, làm ơn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZGEraBWj53j-4","text":"Hi. What can I get for you?","type":"sentence","start_time":"00:00:19.940","end_time":"00:00:22.360","unapproved":1,"translation":{"text":"Chào. Tôi có thể nhận được gì cho bạn?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZGEraBWj53j-5","text":"I’d like a number 1, please.","type":"sentence","start_time":"00:00:22.500","end_time":"00:00:26.300","unapproved":1,"translation":{"text":"Tôi thích như số 1, làm ơn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZGEraBWj53j-6","text":"What kind of drink?","type":"sentence","start_time":"00:00:26.310","end_time":"00:00:28.210","unapproved":1,"translation":{"text":"Uống loại gì?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZGEraBWj53j-7","text":"Coke without ice.","type":"sentence","start_time":"00:00:28.210","end_time":"00:00:30.220","unapproved":1,"translation":{"text":"Than cốc không có đá.","google_translate":1},"recordStat":{"voice_text":"