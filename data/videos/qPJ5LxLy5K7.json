right":0,"wrong":0}},{"id":"qPJ5LxLy5K7-44","text":"Turn around!","type":"sentence","start_time":"00:04:32.700","end_time":"00:04:34.460","unapproved":1,"translation":{"text":"Quay lại!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5LxLy5K7-45","text":"This is a backbone","type":"sentence","start_time":"00:04:36.680","end_time":"00:04:40.220","unapproved":1,"translation":{"text":"Đây là một xương sống","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5LxLy5K7-46","text":"or a spine.","type":"sentence","start_time":"00:04:43.040","end_time":"00:04:45.820","unapproved":1,"translation":{"text":"hoặc một cột sống.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5LxLy5K7-47","text":"And these","type":"sentence","start_time":"00:04:46.960","end_time":"00:04:48.980","unapproved":1,"translation":{"text":"Và những","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5LxLy5K7-48","text":"are vertebras.","type":"sentence","start_time":"00:04:50.520","end_time":"00:04:52.540","unapproved":1,"translation":{"text":"là động vật có xương sống.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5LxLy5K7-49","text":"Vertebras.","type":"sentence","start_time":"00:04:57.760","end_time":"00:04:59.220","unapproved":1,"translation":{"text":"Động vật có xương sống.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5LxLy5K7-50","text":"And this is a shoulder blade.","type":"sentence","start_time":"00:05:07.320","end_time":"00:05:11.100","unapproved":1,"translation":{"text":"Và đây là một xương bả vai.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5LxLy5K7-51","text":"A shoulder blade.","type":"sentence","start_time":"00:05:13.4