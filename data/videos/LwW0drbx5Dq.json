{"data":[{"id":"LwW0drbx5Dq-1","text":"We will give you some running tips","type":"sentence","start_time":"00:00:12.660","end_time":"00:00:15.320","unapproved":1,"translation":{"text":"Chúng tôi sẽ cung cấp cho bạn một số mẹo chạy","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-2","text":"how to get in shape.","type":"sentence","start_time":"00:00:15.520","end_time":"00:00:18.520","unapproved":1,"translation":{"text":"làm thế nào để có được hình dạng","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-3","text":"Get new running shoes","type":"sentence","start_time":"00:00:23.780","end_time":"00:00:26.860","unapproved":1,"translation":{"text":"Nhận giày chạy mới","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-4","text":"with a good-quality shoe sole.","type":"sentence","start_time":"00:00:27.480","end_time":"00:00:31.000","unapproved":1,"translation":{"text":"với đế giày chất lượng tốt.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-5","text":"Put your shoes on.","type":"sentence","start_time":"00:00:33.280","end_time":"00:00:36.340","unapproved":1,"translation":{"text":"Mang giày vào.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-6","text":"And lace up your sneakers.","type":"sentence","start_time":"00:00:36.860","end_time":"00:00:40.260","unapproved":1,"translation":{"text":"Và thắt dây giày thể thao của bạn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-7","text":"Get a running outfit.","type":"sentence","start_time":"00:00:46.320","end_time":"00:00:49.200","unapproved":1,"translation":{"text":"Nhận một bộ trang phục chạy.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-8","text":"Warm up!","type":"sentence","start_time":"00:00:53.500","end_time":"00:00:56.080","unapproved":1,"translation":{"text":"Ấm lên!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-9","text":"And start jogging.","type":"sentence","start_time":"00:01:04.360","end_time":"00:01:07.580","unapproved":1,"translation":{"text":"Và bắt đầu chạy bộ.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-10","text":"Loose your shoulders","type":"sentence","start_time":"00:01:11.920","end_time":"00:01:15.380","unapproved":1,"translation":{"text":"Mất vai","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-11","text":"but stay straight.","type":"sentence","start_time":"00:01:16.700","end_time":"00:01:19.480","unapproved":1,"translation":{"text":"nhưng giữ thẳng","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-12","text":"and just enjoy your run.","type":"sentence","start_time":"00:01:22.120","end_time":"00:01:25.180","unapproved":1,"translation":{"text":"và chỉ cần tận hưởng chạy của bạn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-13","text":"I am jumping.","type":"sentence","start_time":"00:01:25.940","end_time":"00:01:28.960","unapproved":1,"translation":{"text":"Tôi đang nhảy.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-14","text":"Come on!","type":"sentence","start_time":"00:01:33.560","end_time":"00:01:35.020","unapproved":1,"translation":{"text":"Nào!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-15","text":"Speed up!","type":"sentence","start_time":"00:01:35.220","end_time":"00:01:36.700","unapproved":1,"translation":{"text":"Tăng tốc!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-16","text":"Ok ok ok!","type":"sentence","start_time":"00:01:41.760","end_time":"00:01:43.080","unapproved":1,"translation":{"text":"Được được được!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-17","text":"Slow down! Slow down!","type":"sentence","start_time":"00:01:43.240","end_time":"00:01:44.800","unapproved":1,"translation":{"text":"Chậm lại! Chậm lại!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-18","text":"Are you breathing properly?","type":"sentence","start_time":"00:01:52.120","end_time":"00:01:55.260","unapproved":1,"translation":{"text":"Bạn đang thở đúng cách?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-19","text":"I am breathing.","type":"sentence","start_time":"00:01:56.840","end_time":"00:01:58.540","unapproved":1,"translation":{"text":"Tôi đang thở","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-20","text":"We are running downhill.","type":"sentence","start_time":"00:02:02.260","end_time":"00:02:04.760","unapproved":1,"translation":{"text":"Chúng tôi đang chạy xuống dốc.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-21","text":"Downhill...","type":"sentence","start_time":"00:02:05.020","end_time":"00:02:06.320","unapproved":1,"translation":{"text":"Xuống dốc ...","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-22","text":"Lukas! Sprint uphill!","type":"sentence","start_time":"00:02:11.320","end_time":"00:02:14.400","unapproved":1,"translation":{"text":"Lukas! Nước rút khó khăn!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-23","text":"Sprint uphill!","type":"sentence","start_time":"00:02:15.760","end_time":"00:02:16.800","unapproved":1,"translation":{"text":"Nước rút khó khăn!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-24","text":"Are you breathing properly?","type":"sentence","start_time":"00:02:24.760","end_time":"00:02:26.680","unapproved":1,"translation":{"text":"Bạn đang thở đúng cách?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-25","text":"I am trying.","type":"sentence","start_time":"00:02:27.500","end_time":"00:02:28.880","unapproved":1,"translation":{"text":"Tôi đang cố gắng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-26","text":"And when your workout is over","type":"sentence","start_time":"00:02:31.100","end_time":"00:02:34.580","unapproved":1,"translation":{"text":"Và khi tập luyện của bạn kết thúc","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-27","text":"stretch yourself!","type":"sentence","start_time":"00:02:35.140","end_time":"00:02:36.960","unapproved":1,"translation":{"text":"vươn mình!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-28","text":"Can you show me your shoe sole?!","type":"sentence","start_time":"00:02:56.220","end_time":"00:02:59.140","unapproved":1,"translation":{"text":"Bạn có thể chỉ cho tôi đế giày của bạn?!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drbx5Dq-29","text":"Bad luck!","type":"sentence","start_time":"00:03:03.100","end_time":"00:03:04.860","unapproved":1,"translation":{"text":"Xui xẻo!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}}],"view"