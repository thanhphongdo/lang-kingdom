ved":1,"translation":{"text":"Có lẽ chúng ta có thể vẽ điều đó.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"WLEajoYM5Xq-37","text":"Is it, like tornadoes and thunderstorms or...","type":"sentence","start_time":"00:01:01.480","end_time":"00:01:04.772","unapproved":1,"translation":{"text":"Có phải, như lốc xoáy và giông bão hay ...","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"WLEajoYM5Xq-38","text":"- Just candy cotton cloud with chocolate milk ring.","type":"sentence","start_time":"00:01:04.772","end_time":"00:01:08.939","unapproved":1,"translation":{"text":"- Chỉ cần kẹo bông gòn với vòng sữa sô cô la.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"WLEajoYM5Xq-39","text":"- Cotton candy clouds?","type":"sentence","start_time":"00:01:09.857","end_time":"00:01:11.261","unapproved":1,"translation":{"text":"- Mây kẹo bông?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"WLEajoYM5Xq-40","text":"- Yeah.","type":"sentence","start_time":"00:01:11.261","end_time":"00:01:12.161","unapproved":1,"translation":{"text":"- Ừ.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"WLEajoYM5Xq-41","text":"- [Koji] What else did you say?","type":"sentence","start_time":"00:01:12.161","end_time":"00:01:13.239","unapproved":1,"translation":{"text":"- [Koji] Bạn còn nói gì nữa không?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"WLEajoYM5Xq-42","text":"- [Ernie] Chocolate milk ring.","type":"sentence","start_time":"00:01:13.239","end_time":"00:01:14.881","unapproved":1,"translation":{"text":"- [Ernie] Nhẫn sữa sô cô la.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"WLEajoYM5Xq-43","text":"- [Koji] Here's the chocolate milk ring.","