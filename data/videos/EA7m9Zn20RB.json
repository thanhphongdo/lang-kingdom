approved":1,"translation":{"text":"Cô ấy rất sợ hãi","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA7m9Zn20RB-29","text":"so I want to be more sneaky.","type":"sentence","start_time":"00:01:17.250","end_time":"00:01:18.637","unapproved":1,"translation":{"text":"vì vậy tôi muốn được lén lút hơn","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA7m9Zn20RB-30","text":"It's actually pretty funny","type":"sentence","start_time":"00:01:18.637","end_time":"00:01:19.665","unapproved":1,"translation":{"text":"Nó thực sự khá buồn cười","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA7m9Zn20RB-31","text":"when she go like Ahhhh!","type":"sentence","start_time":"00:01:19.665","end_time":"00:01:20.990","unapproved":1,"translation":{"text":"khi cô ấy đi như Ahhhh!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA7m9Zn20RB-32","text":"What was that?","type":"sentence","start_time":"00:01:20.990","end_time":"00:01:22.260","unapproved":1,"translation":{"text":"Đó là cái gì","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA7m9Zn20RB-33","text":"It's actually pretty funny.","type":"sentence","start_time":"00:01:22.260","end_time":"00:01:24.142","unapproved":1,"translation":{"text":"Nó thực sự khá buồn cười.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA7m9Zn20RB-34","text":"Is that an (whispering)!","type":"sentence","start_time":"00:01:24.142","end_time":"00:01:26.694","unapproved":1,"translation":{"text":"Có phải đó là một (thì thầm)!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA7m9Zn20RB-35","text":"Even though it doesn't look like I got a haircut,","type":"sentence","start_time":"00:01:26.694","end_time":"00:01:28.900"