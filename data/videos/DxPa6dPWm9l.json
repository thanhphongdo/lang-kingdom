{"data":[{"id":"DxPa6dPWm9l-1","text":"We are in the lab.","type":"sentence","start_time":"00:00:15.100","end_time":"00:00:16.760","unapproved":1,"translation":{"text":"Chúng tôi đang ở trong phòng thí nghiệm.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-2","text":"We are scientists.","type":"sentence","start_time":"00:00:16.960","end_time":"00:00:20.680","unapproved":1,"translation":{"text":"Chúng tôi là những nhà khoa học.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-3","text":"These are scales.","type":"sentence","start_time":"00:00:21.980","end_time":"00:00:25.160","unapproved":1,"translation":{"text":"Đây là những cái cân.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-4","text":"This is a microscope.","type":"sentence","start_time":"00:00:30.940","end_time":"00:00:34.300","unapproved":1,"translation":{"text":"Đây là một kính hiển vi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-5","text":"And what about this?","type":"sentence","start_time":"00:00:38.800","end_time":"00:00:40.920","unapproved":1,"translation":{"text":"Còn cái này thì sao?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-6","text":"This is a thermometer.","type":"sentence","start_time":"00:00:43.000","end_time":"00:00:46.380","unapproved":1,"translation":{"text":"Đây là một nhiệt kế.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-7","text":"Ok, ok, ok.","type":"sentence","start_time":"00:00:48.720","end_time":"00:00:50.100","unapproved":1,"translation":{"text":"Được được được.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-8","text":"What about this?","type":"sentence","start_time":"00:00:50.600","end_time":"00:00:52.420","unapproved":1,"translation":{"text":"Cái này thì sao?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-9","text":"This is a battery.","type":"sentence","start_time":"00:00:53.100","end_time":"00:00:57.060","unapproved":1,"translation":{"text":"Đây là một pin.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-10","text":"And now three states of water:","type":"sentence","start_time":"00:01:01.800","end_time":"00:01:05.380","unapproved":1,"translation":{"text":"Và bây giờ ba trạng thái của nước:","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-11","text":"This is liquid water.","type":"sentence","start_time":"00:01:06.060","end_time":"00:01:09.200","unapproved":1,"translation":{"text":"Đây là nước lỏng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-12","text":"liquid...","type":"sentence","start_time":"00:01:12.540","end_time":"00:01:13.660","unapproved":1,"translation":{"text":"chất lỏng...","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-13","text":"The ice...","type":"sentence","start_time":"00:01:17.120","end_time":"00:01:18.720","unapproved":1,"translation":{"text":"Băng ...","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-14","text":"... is solid.","type":"sentence","start_time":"00:01:19.800","end_time":"00:01:21.820","unapproved":1,"translation":{"text":"... Là vững chắc.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-15","text":"I am boiling the water.","type":"sentence","start_time":"00:01:32.480","end_time":"00:01:35.720","unapproved":1,"translation":{"text":"Tôi đang đun nước.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-16","text":"The boiling water turns into steam.","type":"sentence","start_time":"00:01:37.900","end_time":"00:01:42.860","unapproved":1,"translation":{"text":"Nước sôi biến thành hơi nước.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-17","text":"The steam is gas.","type":"sentence","start_time":"00:01:43.700","end_time":"00:01:48.580","unapproved":1,"translation":{"text":"Hơi nước là khí.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-18","text":"Do you like science?","type":"sentence","start_time":"00:01:52.560","end_time":"00:01:54.580","unapproved":1,"translation":{"text":"Bạn có thích khoa học không?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-19","text":"We will show you some inventions.","type":"sentence","start_time":"00:01:54.740","end_time":"00:01:57.520","unapproved":1,"translation":{"text":"Chúng tôi sẽ cho bạn thấy một số phát minh.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-20","text":"Let's go to the SCIENCE CENTRE.","type":"sentence","start_time":"00:01:57.620","end_time":"00:02:01.060","unapproved":1,"translation":{"text":"Hãy đến TRUNG TÂM KHOA HỌC.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-21","text":"This is a steam engine.","type":"sentence","start_time":"00:02:06.060","end_time":"00:02:09.720","unapproved":1,"translation":{"text":"Đây là một động cơ hơi nước.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-22","text":"A steam locomotive.","type":"sentence","start_time":"00:02:16.680","end_time":"00:02:19.980","unapproved":1,"translation":{"text":"Một đầu máy hơi nước.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-23","text":"The steam locomotives were developed in the first half of the nineteenth century.","type":"sentence","start_time":"00:02:20.540","end_time":"00:02:29.560","unapproved":1,"translation":{"text":"Đầu máy xe lửa hơi nước được phát triển vào nửa đầu thế kỷ XIX.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-24","text":"A wheel.","type":"sentence","start_time":"00:02:31.860","end_time":"00:02:33.920","unapproved":1,"translation":{"text":"Một bánh xe.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-25","text":"The wheel is one of the most important inventions.","type":"sentence","start_time":"00:02:34.920","end_time":"00:02:40.280","unapproved":1,"translation":{"text":"Bánh xe là một trong những phát minh quan trọng nhất.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-26","text":"What is it?","type":"sentence","start_time":"00:02:46.660","end_time":"00:02:49.140","unapproved":1,"translation":{"text":"Nó là gì?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-27","text":"These are magnets.","type":"sentence","start_time":"00:02:50.580","end_time":"00:02:52.900","unapproved":1,"translation":{"text":"Đây là những nam châm.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-28","text":"Really?","type":"sentence","start_time":"00:02:53.620","end_time":"00:02:55.080","unapproved":1,"translation":{"text":"Có thật không?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-29","text":"You see...","type":"sentence","start_time":"00:03:01.200","end_time":"00:03:02.260","unapproved":1,"translation":{"text":"Bạn thấy ...","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-30","text":"This piece of metal is magnetic.","type":"sentence","start_time":"00:03:03.100","end_time":"00:03:07.780","unapproved":1,"translation":{"text":"Mảnh kim loại này là từ tính.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-31","text":"Come on!","type":"sentence","start_time":"00:03:15.420","end_time":"00:03:17.100","unapproved":1,"translation":{"text":"Nào!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-32","text":"Electricity.","type":"sentence","start_time":"00:03:22.420","end_time":"00:03:25.040","unapproved":1,"translation":{"text":"Điện lực.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-33","text":"This is a trolleybus.","type":"sentence","start_time":"00:03:27.120","end_time":"00:03:29.400","unapproved":1,"translation":{"text":"Đây là một chiếc xe đẩy.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-34","text":"Yes. The trolleybus is powered by electricity.","type":"sentence","start_time":"00:03:31.240","end_time":"00:03:35.940","unapproved":1,"translation":{"text":"Đúng. Xe đẩy được cung cấp điện.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-35","text":"This is a submarine.","type":"sentence","start_time":"00:03:43.580","end_time":"00:03:47.840","unapproved":1,"translation":{"text":"Đây là một chiếc tàu ngầm.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-36","text":"This is an excavator.","type":"sentence","start_time":"00:03:52.780","end_time":"00:03:55.340","unapproved":1,"translation":{"text":"Đây là một máy xúc.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-37","text":"This is a crane.","type":"sentence","start_time":"00:04:07.020","end_time":"00:04:09.320","unapproved":1,"translation":{"text":"Đây là một cần cẩu.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-38","text":"The crane can lift the weight having 32 tonnes.","type":"sentence","start_time":"00:04:11.500","end_time":"00:04:18.620","unapproved":1,"translation":{"text":"Cần cẩu có thể nâng trọng lượng có 32 tấn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-39","text":"This is a metal detector.","type":"sentence","start_time":"00:04:21.780","end_time":"00:04:25.660","unapproved":1,"translation":{"text":"Đây là một máy dò kim loại.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-40","text":"a metal detector","type":"sentence","start_time":"00:04:26.080","end_time":"00:04:28.700","unapproved":1,"translation":{"text":"máy dò kim loại","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-41","text":"Mister?!","type":"sentence","start_time":"00:04:32.340","end_time":"00:04:34.160","unapproved":1,"translation":{"text":"Quí ông?!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-42","text":"This is an X-ray.","type":"sentence","start_time":"00:04:47.420","end_time":"00:04:49.560","unapproved":1,"translation":{"text":"Đây là một tia X.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-43","text":"Your suitcases please.","type":"sentence","start_time":"00:04:52.060","end_time":"00:04:55.360","unapproved":1,"translation":{"text":"Vui lòng cho biết vali của bạn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-44","text":"Ok. Next.","type":"sentence","start_time":"00:05:01.940","end_time":"00:05:03.860","unapproved":1,"translation":{"text":"Đồng ý. Kế tiếp.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-45","text":"This is an X-ray.","type":"sentence","start_time":"00:05:06.940","end_time":"00:05:08.660","unapproved":1,"translation":{"text":"Đây là một tia X.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-46","text":"It uses radiation.","type":"sentence","start_time":"00:05:08.740","end_time":"00:05:11.640","unapproved":1,"translation":{"text":"Nó sử dụng bức xạ.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6dPWm9l-47","text":"Mister, come with me!","type":"sentence","start_time":"00:05:14.400","end_time":"00:05:17.580","unapproved":1,"translation":{"text":"Thưa ngài, hãy đến với tôi!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}}],"view":"1","comment":"0","like":"0","dislike":"0","like_status":0,"is_news_a_minute":0,"slideshow":0,"id":"DxPa6dPWm9l","url":"https://www.youtube.com/watch?v=B84XxZhONow","title":"Science - English vocabulary","thumbnail":"https://i.ytimg.com/vi/B84XxZhONow/maxresdefault.jpg","mp4":0,"comment_id":null,"duration":"00:05:52","is_new":null}