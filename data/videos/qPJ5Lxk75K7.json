{"data":[{"id":"qPJ5Lxk75K7-1","text":"We will teach you the pub vocab.","type":"sentence","start_time":"00:00:03.720","end_time":"00:00:08.180","unapproved":1,"translation":{"text":"Chúng tôi sẽ dạy cho bạn các vocab pub.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-2","text":"Hi. This is MonkeyEnglish.","type":"sentence","start_time":"00:00:15.251","end_time":"00:00:17.091","unapproved":1,"translation":{"text":"Chào. Đây là MonkeyEnglish.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-3","text":"My name is Lukas.","type":"sentence","start_time":"00:00:17.917","end_time":"00:00:19.037","unapproved":1,"translation":{"text":"Tôi tên là Lukas","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-4","text":"I am Lukas. We are teachers.","type":"sentence","start_time":"00:00:21.400","end_time":"00:00:24.200","unapproved":1,"translation":{"text":"Tôi là Lukas. Chúng tôi là giáo viên.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-5","text":"MonkeyEnglish is useful and simple.","type":"sentence","start_time":"00:00:24.880","end_time":"00:00:28.220","unapproved":1,"translation":{"text":"MonkeyEnglish rất hữu ích và đơn giản.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-6","text":"We are going for a pint.","type":"sentence","start_time":"00:00:29.020","end_time":"00:00:31.760","unapproved":1,"translation":{"text":"Chúng tôi sẽ cho một pint.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-7","text":"For a beer.","type":"sentence","start_time":"00:00:31.880","end_time":"00:00:32.680","unapproved":1,"translation":{"text":"Cho một ly bia.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-8","text":"Let's go!","type":"sentence","start_time":"00:00:34.180","end_time":"00:00:34.780","unapproved":1,"translation":{"text":"Đi nào!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-9","text":"The pub...come in!","type":"sentence","start_time":"00:00:35.020","end_time":"00:00:36.340","unapproved":1,"translation":{"text":"Quán rượu ... vào đi!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-10","text":"Are you ready to order?","type":"sentence","start_time":"00:00:41.560","end_time":"00:00:43.580","unapproved":1,"translation":{"text":"Bạn đã sẵn sàng để đặt chưa?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-11","text":"I am ready.","type":"sentence","start_time":"00:00:44.280","end_time":"00:00:45.460","unapproved":1,"translation":{"text":"Tôi sẵn sàng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-12","text":"May we have two beers?","type":"sentence","start_time":"00:00:47.800","end_time":"00:00:49.340","unapproved":1,"translation":{"text":"Chúng ta có thể có hai loại bia không?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-13","text":"Great!","type":"sentence","start_time":"00:00:49.480","end_time":"00:00:49.980","unapproved":1,"translation":{"text":"Tuyệt quá!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-14","text":"Thank you.","type":"sentence","start_time":"00:00:58.480","end_time":"00:00:59.200","unapproved":1,"translation":{"text":"Cảm ơn bạn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-15","text":"May I ask you?","type":"sentence","start_time":"00:01:04.580","end_time":"00:01:05.820","unapproved":1,"translation":{"text":"Tôi có thể hỏi bạn?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-16","text":"Sorry? Mrs!","type":"sentence","start_time":"00:01:06.320","end_time":"00:01:07.660","unapproved":1,"translation":{"text":"Lấy làm tiếc? Bà!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-17","text":"This is Pilsen beer?","type":"sentence","start_time":"00:01:07.760","end_time":"00:01:09.120","unapproved":1,"translation":{"text":"Đây là bia Pilsen?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-18","text":"Yes. Pilsen beer.","type":"sentence","start_time":"00:01:09.340","end_time":"00:01:10.620","unapproved":1,"translation":{"text":"Đúng. Bia Pilsen.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-19","text":"Great. Thank you.","type":"sentence","start_time":"00:01:10.800","end_time":"00:01:11.753","unapproved":1,"translation":{"text":"Tuyệt quá. Cảm ơn bạn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-20","text":"Ok.","type":"sentence","start_time":"00:01:11.760","end_time":"00:01:12.260","unapproved":1,"translation":{"text":"Đồng ý.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-21","text":"Cheers!","type":"sentence","start_time":"00:01:12.500","end_time":"00:01:15.140","unapproved":1,"translation":{"text":"Chúc mừng!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-22","text":"I like czech beer.","type":"sentence","start_time":"00:01:21.420","end_time":"00:01:23.240","unapproved":1,"translation":{"text":"Tôi thích bia czech.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-23","text":"This is a head.","type":"sentence","start_time":"00:01:25.920","end_time":"00:01:27.260","unapproved":1,"translation":{"text":"Đây là một cái đầu.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-24","text":"This is a pint of beer.","type":"sentence","start_time":"00:01:31.480","end_time":"00:01:34.040","unapproved":1,"translation":{"text":"Đây là một cốc bia.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-25","text":"It's half litre of beer.","type":"sentence","start_time":"00:01:36.300","end_time":"00:01:39.620","unapproved":1,"translation":{"text":"Đó là nửa lít bia.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-26","text":"What do you think about czech music?","type":"sentence","start_time":"00:01:44.360","end_time":"00:01:46.940","unapproved":1,"translation":{"text":"Bạn nghĩ gì về âm nhạc czech?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-27","text":"It is not funky.","type":"sentence","start_time":"00:01:59.580","end_time":"00:02:01.160","unapproved":1,"translation":{"text":"Nó không phải là sôi nổi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-28","text":"It is not funky but it slowly comes under your skin...","type":"sentence","start_time":"00:02:02.140","end_time":"00:02:08.460","unapproved":1,"translation":{"text":"Nó không phải là sôi nổi nhưng nó từ từ đến dưới da của bạn ...","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-29","text":"...like the czech beer.","type":"sentence","start_time":"00:02:11.660","end_time":"00:02:14.260","unapproved":1,"translation":{"text":"... Giống như bia Séc.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-30","text":"Cheers!","type":"sentence","start_time":"00:02:17.880","end_time":"00:02:18.820","unapproved":1,"translation":{"text":"Chúc mừng!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-31","text":"Pilsner Urquell is manufactured in Pilsen.","type":"sentence","start_time":"00:02:20.000","end_time":"00:02:24.780","unapproved":1,"translation":{"text":"Pilsner Urquell được sản xuất tại Pilsen.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-32","text":"Pilsen.","type":"sentence","start_time":"00:02:25.680","end_time":"00:02:26.540","unapproved":1,"translation":{"text":"Pilsen.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-33","text":"What is it?","type":"sentence","start_time":"00:02:29.540","end_time":"00:02:31.380","unapproved":1,"translation":{"text":"Nó là gì?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-34","text":"This is a beermat.","type":"sentence","start_time":"00:02:33.920","end_time":"00:02:36.280","unapproved":1,"translation":{"text":"Đây là một cái beermat.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-35","text":"The taste of beer is bitter.","type":"sentence","start_time":"00:02:43.000","end_time":"00:02:46.360","unapproved":1,"translation":{"text":"Hương vị của bia là đắng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-36","text":"Lukas. Beer is a drink.","type":"sentence","start_time":"00:02:52.120","end_time":"00:02:55.720","unapproved":1,"translation":{"text":"Lukas. Bia là một thức uống.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-37","text":"No.","type":"sentence","start_time":"00:02:58.840","end_time":"00:02:59.360","unapproved":1,"translation":{"text":"Không.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-38","text":"Beer is a massage.","type":"sentence","start_time":"00:03:00.205","end_time":"00:03:02.685","unapproved":1,"translation":{"text":"Bia là một massage.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-39","text":"It's time to go.","type":"sentence","start_time":"00:03:08.080","end_time":"00:03:10.060","unapproved":1,"translation":{"text":"Đến lúc phải đi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-40","text":"Bill please.","type":"sentence","start_time":"00:03:14.120","end_time":"00:03:17.700","unapproved":1,"translation":{"text":"Xin vui lòng hóa đơn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-41","text":"Can you lend me some money?","type":"sentence","start_time":"00:03:27.260","end_time":"00:03:29.320","unapproved":1,"translation":{"text":"Bạn có thể cho tôi mượn một ít tiền không?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-42","text":"Yes. Great! Keep change.","type":"sentence","start_time":"00:03:31.620","end_time":"00:03:34.500","unapproved":1,"translation":{"text":"Đúng. Tuyệt quá! Giữ sự thay đổi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-43","text":"Keep change.","type":"sentence","start_time":"00:03:34.940","end_time":"00:03:36.440","unapproved":1,"translation":{"text":"Giữ sự thay đổi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-44","text":"This is a bill.","type":"sentence","start_time":"00:03:40.400","end_time":"00:03:42.600","unapproved":1,"translation":{"text":"Đây là một hóa đơn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-45","text":"The bill is really long but we only had two beers.","type":"sentence","start_time":"00:03:46.400","end_time":"00:03:52.320","unapproved":1,"translation":{"text":"Hóa đơn rất dài nhưng chúng tôi chỉ có hai loại bia.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qPJ5Lxk75K7-46","text":"Together.","type":"sentence","start_time":"00:03:52.540","end_time":"00:03:53.780","unapproved":1,"translation":{"text":"Cùng với nhau.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}}],"view":"0","comment":"0","like":"0","dislike":"0","like_status":0,"is_news_a_minute":0,"slideshow":0,"id":"qPJ5Lxk75K7","url":"https://www.youtube.com/watch?v=3VR2dPG6v_Q","title":"Pub - english vocabulary","thumbnail":"https://i.ytimg.com/vi/3VR2dPG6v_Q/maxresdefault.jpg","mp4":0,"comment_id":null,"duration":"00:04:25","is_new":null}