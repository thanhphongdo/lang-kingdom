{"data":[{"id":"b2JaRdZpmdn-1","text":"Hi. This is MonkeyEnglish.","type":"sentence","start_time":"00:00:12.060","end_time":"00:00:14.020","unapproved":1,"translation":{"text":"Chào. Đây là MonkeyEnglish.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"b2JaRdZpmdn-2","text":"My name is Lukas. I am a teacher.","type":"sentence","start_time":"00:00:14.800","end_time":"00:00:18.280","unapproved":1,"translation":{"text":"Tôi tên là Lukas Tôi là một giáo viên.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"b2JaRdZpmdn-3","text":"MonkeyEnglish is simple and useful.","type":"sentence","start_time":"00:00:18.480","end_time":"00:00:21.920","unapproved":1,"translation":{"text":"MonkeyEnglish rất đơn giản và hữu ích.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"b2JaRdZpmdn-4","text":"Airport.","type":"sentence","start_time":"00:00:25.080","end_time":"00:00:26.320","unapproved":1,"translation":{"text":"Sân bay.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"b2JaRdZpmdn-5","text":"This is my luggage.","type":"sentence","start_time":"00:00:28.060","end_time":"00:00:29.820","unapproved":1,"translation":{"text":"Đây là hành lý của tôi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"b2JaRdZpmdn-6","text":"This is a bag.","type":"sentence","start_time":"00:00:31.440","end_time":"00:00:33.400","unapproved":1,"translation":{"text":"Đây là một cái túi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"b2JaRdZpmdn-7","text":"This is luggage.","type":"sentence","start_time":"00:00:34.500","end_time":"00:00:36.140","unapproved":1,"translation":{"text":"Đây là hành lý.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"b2JaRdZpmdn-8","text