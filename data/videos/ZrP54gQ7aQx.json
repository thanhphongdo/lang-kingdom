{"data":[{"id":"ZrP54gQ7aQx-1","text":"This is a cigarette pack.","type":"sentence","start_time":"00:00:22.380","end_time":"00:00:24.860","unapproved":1,"translation":{"text":"Đây là một gói thuốc lá.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-2","text":"You are smoking!","type":"sentence","start_time":"00:00:26.620","end_time":"00:00:28.760","unapproved":1,"translation":{"text":"Bạn đang hút thuốc lá!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-3","text":"Yes, rarely.","type":"sentence","start_time":"00:00:29.120","end_time":"00:00:31.120","unapproved":1,"translation":{"text":"Vâng, hiếm khi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-4","text":"It's a joint?","type":"sentence","start_time":"00:00:38.060","end_time":"00:00:40.500","unapproved":1,"translation":{"text":"Đó là một khớp?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-5","text":"No, it is a cigarette.","type":"sentence","start_time":"00:00:41.120","end_time":"00:00:43.600","unapproved":1,"translation":{"text":"Không, nó là một điếu thuốc.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-6","text":"Can you light my cigarette?","type":"sentence","start_time":"00:00:44.520","end_time":"00:00:47.260","unapproved":1,"translation":{"text":"Bạn có thể châm thuốc lá của tôi?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-7","text":"No!","type":"sentence","start_time":"00:00:48.520","end_time":"00:00:49.780","unapproved":1,"translation":{"text":"Không!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-8","text":"I can lend you my lighter.","type":"sentence","start_time":"00:00:52.100","end_time":"00:00:56.040","unapproved":1,"translation":{"text":"Tôi có thể cho bạn mượn cái bật lửa của tôi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-9","text":"This is a lighter.","type":"sentence","start_time":"00:00:58.800","end_time":"00:01:01.720","unapproved":1,"translation":{"text":"Đây là một cái bật lửa.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-10","text":"I am lighting my cigarette.","type":"sentence","start_time":"00:01:08.180","end_time":"00:01:11.140","unapproved":1,"translation":{"text":"Tôi đang châm thuốc.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-11","text":"The smoke...","type":"sentence","start_time":"00:01:19.040","end_time":"00:01:21.440","unapproved":1,"translation":{"text":"Khói ...","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-12","text":"It's stinking.","type":"sentence","start_time":"00:01:21.680","end_time":"00:01:24.260","unapproved":1,"translation":{"text":"Nó hôi thối.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-13","text":"You are inhaling the smoke?","type":"sentence","start_time":"00:01:30.520","end_time":"00:01:33.780","unapproved":1,"translation":{"text":"Bạn đang hít khói?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-14","text":"You see!","type":"sentence","start_time":"00:01:40.300","end_time":"00:01:41.340","unapproved":1,"translation":{"text":"Bạn thấy đấy!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-15","text":"You are coughing.","type":"sentence","start_time":"00:01:41.880","end_time":"00:01:44.100","unapproved":1,"translation":{"text":"Bạn đang ho.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-16","text":"I know. It is unhealthy.","type":"sentence","start_time":"00:01:45.120","end_time":"00:01:48.140","unapproved":1,"translation":{"text":"Tôi biết. Nó không tốt cho sức khoẻ.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-17","text":"Lukas! I am advising you: Stop smoking!","type":"sentence","start_time":"00:01:49.800","end_time":"00:01:55.200","unapproved":1,"translation":{"text":"Lukas! Tôi khuyên bạn: Hãy ngừng hút thuốc!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-18","text":"And you know!","type":"sentence","start_time":"00:02:00.240","end_time":"00:02:02.580","unapproved":1,"translation":{"text":"Và bạn biết đấy!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-19","text":"There is a smoking ban.","type":"sentence","start_time":"00:02:03.740","end_time":"00:02:06.840","unapproved":1,"translation":{"text":"Có lệnh cấm hút thuốc.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-20","text":"What does it mean?","type":"sentence","start_time":"00:02:07.400","end_time":"00:02:09.380","unapproved":1,"translation":{"text":"Nó có nghĩa là gì?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-21","text":"You can't smoke here!","type":"sentence","start_time":"00:02:10.800","end_time":"00:02:12.660","unapproved":1,"translation":{"text":"Bạn không thể hút thuốc ở đây!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-22","text":"It's a bus stop.","type":"sentence","start_time":"00:02:12.880","end_time":"00:02:14.660","unapproved":1,"translation":{"text":"Đó là một trạm xe buýt.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-23","text":"Smoking is prohibited.","type":"sentence","start_time":"00:02:18.800","end_time":"00:02:21.500","unapproved":1,"translation":{"text":"Hút thuốc bị cấm.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-24","text":"The police can give you a fine!","type":"sentence","start_time":"00:02:22.700","end_time":"00:02:25.620","unapproved":1,"translation":{"text":"Cảnh sát có thể cho bạn một khoản tiền phạt!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-25","text":"A fine?","type":"sentence","start_time":"00:02:26.760","end_time":"00:02:27.960","unapproved":1,"translation":{"text":"Phạt không?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-26","text":"What is it?","type":"sentence","start_time":"00:02:28.600","end_time":"00:02:29.840","unapproved":1,"translation":{"text":"Nó là gì?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-27","text":"You pay money for smoking at the bus stop.","type":"sentence","start_time":"00:02:30.160","end_time":"00:02:33.240","unapproved":1,"translation":{"text":"Bạn trả tiền cho việc hút thuốc ở trạm xe buýt.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-28","text":"Grind it out!","type":"sentence","start_time":"00:02:34.540","end_time":"00:02:37.020","unapproved":1,"translation":{"text":"Nghiền nó ra!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-29","text":"Why did you bring me to this place?","type":"sentence","start_time":"00:02:56.060","end_time":"00:02:58.900","unapproved":1,"translation":{"text":"Tại sao bạn đưa tôi đến nơi này?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-30","text":"Why? Where are we?","type":"sentence","start_time":"00:02:58.960","end_time":"00:03:01.280","unapproved":1,"translation":{"text":"Tại sao? Chúng ta ở đâu?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-31","text":"This is a cemetery.","type":"sentence","start_time":"00:03:01.760","end_time":"00:03:03.500","unapproved":1,"translation":{"text":"Đây là một nghĩa trang.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-32","text":"Yes, that's right.","type":"sentence","start_time":"00:03:03.540","end_time":"00:03:05.600","unapproved":1,"translation":{"text":"Vâng đúng vậy.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-33","text":"Lukas. You are a smoker.","type":"sentence","start_time":"00:03:05.800","end_time":"00:03:08.040","unapproved":1,"translation":{"text":"Lukas. Bạn là người hút thuốc","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-34","text":"Yes, I am a smoker.","type":"sentence","start_time":"00:03:08.420","end_time":"00:03:10.220","unapproved":1,"translation":{"text":"Vâng, tôi là người hút thuốc.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-35","text":"You can die.","type":"sentence","start_time":"00:03:10.580","end_time":"00:03:12.220","unapproved":1,"translation":{"text":"Bạn có thể chết.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-36","text":"Really?","type":"sentence","start_time":"00:03:12.380","end_time":"00:03:13.540","unapproved":1,"translation":{"text":"Có thật không?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-37","text":"You can get lung cancer.","type":"sentence","start_time":"00:03:14.480","end_time":"00:03:17.460","unapproved":1,"translation":{"text":"Bạn có thể bị ung thư phổi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-38","text":"And the lung cancer can kill you.","type":"sentence","start_time":"00:03:18.460","end_time":"00:03:22.120","unapproved":1,"translation":{"text":"Và ung thư phổi có thể giết chết bạn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-39","text":"I am advising you: Stop smoking!","type":"sentence","start_time":"00:03:24.620","end_time":"00:03:28.060","unapproved":1,"translation":{"text":"Tôi khuyên bạn: Hãy ngừng hút thuốc!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-40","text":"But I am addicted.","type":"sentence","start_time":"00:03:28.580","end_time":"00:03:30.540","unapproved":1,"translation":{"text":"Nhưng tôi nghiện.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-41","text":"I need my cigarette.","type":"sentence","start_time":"00:03:30.680","end_time":"00:03:33.180","unapproved":1,"translation":{"text":"Tôi cần thuốc lá của tôi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-42","text":"Lukas, can you see the graves?","type":"sentence","start_time":"00:03:34.520","end_time":"00:03:38.300","unapproved":1,"translation":{"text":"Lukas, bạn có thể nhìn thấy những ngôi mộ?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-43","text":"Yes, there are a lot of graves.","type":"sentence","start_time":"00:03:38.780","end_time":"00:03:41.920","unapproved":1,"translation":{"text":"Vâng, có rất nhiều ngôi mộ.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-44","text":"The graves are full of smokers.","type":"sentence","start_time":"00:03:43.120","end_time":"00:03:46.380","unapproved":1,"translation":{"text":"Những ngôi mộ đầy những người hút thuốc.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-45","text":"Dead smokers.","type":"sentence","start_time":"00:03:46.760","end_time":"00:03:49.260","unapproved":1,"translation":{"text":"Người hút thuốc chết.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-46","text":"I am advising you: Stop smoking!","type":"sentence","start_time":"00:03:51.820","end_time":"00:03:55.320","unapproved":1,"translation":{"text":"Tôi khuyên bạn: Hãy ngừng hút thuốc!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"ZrP54gQ7aQx-47","text":"Ok, you are right.","type":"sentence","start_time":"00:03:56.040","end_time":"00:03:58.720","unapproved":1,"translation":{"text":"Được rồi! Bạn đúng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}}],"view":"0","comment":"0","like":"0","dislike":"0","like_status":0,"is_news_a_minute":0,"slideshow":0,"id":"ZrP54gQ7aQx","url":"https://www.youtube.com/watch?v=atcNx5GCWEk","title":"Smoking - English vocabulary","thumbnail":"https://i.ytimg.com/vi/atcNx5GCWEk/maxresdefault.jpg","mp4":0,"comment_id":null,"duration":"00:04:37","is_new":null}