e":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"qz7aMEVyaO3-30","text":"Good luck!","type":"sentence","start_time":"00:02:30.160","end_time":"00:02:30.880","unapproved":1,"translation":{"text":"Chúc may mắn!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}}],"view":"0","comment":"0","like":"0","dislike":"0","like_status":0,"is_news_a_minute":0,"slideshow":0,"id":"qz7aMEVyaO3","url":"https://www.youtube.com/watch?v=6lMnOVFMrB0","title":"Cross-country skiing","thumbnail":"https://i.ytimg.com/vi/6lMnOVFMrB0/hqdefault.jpg","mp4":0,"comment_id":null,"duration":"00:02:54","is_new":null}