{"data":[{"id":"DxPa6E3E09l-1","text":"Hi. I am Lukas. A teacher.","type":"sentence","start_time":"00:00:12.660","end_time":"00:00:14.780","unapproved":1,"translation":{"text":"sách giáo khoa","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6E3E09l-2","text":"Hi. My name is Lukas as well. I am a teacher.","type":"sentence","start_time":"00:00:14.780","end_time":"00:00:17.840","unapproved":1,"translation":{"text":"nhưng không thể hiểu với người bản xứ","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6E3E09l-3","text":"And this is MonkeyEnglish.","type":"sentence","start_time":"00:00:17.860","end_time":"00:00:20.300","unapproved":1,"translation":{"text":"những người nói tiếng Anh","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6E3E09l-4","text":"MonkeyEnglish is useful and simple.","type":"sentence","start_time":"00:00:20.940","end_time":"00:00:24.480","unapproved":1,"translation":{"text":"đang nói về bạn có muốn","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6E3E09l-5","text":"We'll show you the materials.","type":"sentence","start_time":"00:00:25.540","end_time":"00:00:28.540","unapproved":1,"translation":{"text":"thông thạo tiếng anh","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6E3E09l-6","text":"So Lukas. Let's go!","type":"sentence","start_time":"00:00:29.300","end_time":"00:00:32.340","unapproved":1,"translation":{"text":"nhanh hơn và tốt hơn","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6E3E09l-7","text":"Ok.","type":"sentence","start_time":"00:00:32.560","end_time":"00:00:33.060","unapproved":1,"translation":{"text":"cuộc hội thoại nếu bạn muốn","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6E3E09l-8","text":"It's cold here.","type":"sentence","start_time":"00:00:39.280","end_time":"00:00:40.720","unapproved":1,"translation":{"text":"hiểu biết","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6E3E09l-9","text":"I am freezing.","type":"sentence","start_time":"00:00:41.480","end_time":"00:00:42.740","unapproved":1,"translation":{"text":"và nói tiếng Anh thực sự hơn","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6E3E09l-10","text":"Come on!","type":"sentence","start_time":"00:00:44.360","end_time":"00:00:45.000","unapproved":1,"translation":{"text":"hàng tháng","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6E3E09l-11","text":"Ok.","type":"sentence","start_time":"00:00:45.400","end_time":"00:00:45.900","unapproved":1,"translation":{"text":"nói chuyện tiếng anh","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6E3E09l-12","text":"It's a ceramic flower pot.","type":"sentence","start_time":"00:00:47.700","end_time":"00:00:49.700","unapproved":1,"translation":{"text":"âm thanh trong bài học video","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6E3E09l-13","text":"Fabric.","type":"sentence","start_time":"00:00:53.240","end_time":"00:00:54.380","unapproved":1,"translation":{"text":"là hoàn hảo cho bạn để có được điều đó","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6E3E09l-14","text":"It's a plastic bottle with petrol.","type":"sentence","start_time":"00:00:57.540","end_time":"00:01:00.880","unapproved":1,"translation":{"text":"sách giáo khoa","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa6E3E09l-15","text":"A box made of cardboard.","type":"sentence","start_time":"00:0