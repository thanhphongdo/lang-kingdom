{"data":[{"id":"jz7aylgw0x6-1","text":"\"What I Will\"","type":"sentence","start_time":"00:00:16.260","end_time":"00:00:18.260","unapproved":1,"translation":{"text":"\"Những gì tôi sẽ\"","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"jz7aylgw0x6-2","text":"I will not dance to your war drum.","type":"sentence","start_time":"00:00:18.260","end_time":"00:00:21.260","unapproved":1,"translation":{"text":"Tôi sẽ không nhảy theo trống chiến tranh của bạn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"jz7aylgw0x6-3","text":"I will not lend my soul nor my bones to your war drum.","type":"sentence","start_time":"00:00:22.260","end_time":"00:00:25.260","unapproved":1,"translation":{"text":"Tôi sẽ không cho linh hồn tôi mượn xương của tôi vào trống chiến tranh của bạn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"jz7aylgw0x6-4","text":"I will not dance to that beating.","type":"sentence","start_time":"00:00:25.260","end_time":"00:00:27.260","unapproved":1,"translation":{"text":"Tôi sẽ không nhảy theo nhịp đó.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"jz7aylgw0x6-5","text":"I know that beat.","type":"sentence","start_time":"00:00:27.260","end_time":"00:00:29.260","unapproved":1,"translation":{"text":"Tôi biết nhịp đó.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"jz7aylgw0x6-6","text":"It is lifeless.","type":"sentence","start_time":"00:00:29.260","end_time":"00:00:31.260","unapproved":1,"translation":{"text":"Nó là vô hồn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"jz7aylgw0x6-7","text":"I know intimately that skin you are hitting.","type":"sentence","start_time":"00:00:31.260","end_time":"00:00:34.260","unapproved":1,"translation":{"text":"Tôi biết một cách thân mật rằng làn da bạn đang đánh.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"jz7aylgw0x6-8","text":"It was alive once,","type":"sentence","start_time":"00:00:34.260","end_time":"00:00:36.260","unapproved":1,"translation":{"text":"Nó đã sống một lần,","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"jz7aylgw0x6-9","text":"hunted, stolen,","type":"sentence","start_time":"00:00:36.260","end_time":"00:00:38.260","unapproved":1,"translation":{"text":"săn bắn, đánh cắp,","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"jz7aylgw0x6-10","text":"stretched.","type":"sentence","start_time":"00:00:38.260","end_time":"00:00:40.260","unapproved":1,"translation":{"text":"kéo dài.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"jz7aylgw0x6-11","text":"I will not dance to your drummed-up war.","type":"sentence","start_time":"00:00:40.260","end_time":"00:00:43.260","unapproved":1,"translation":{"text":"Tôi sẽ không nhảy theo cuộc chiến trống của bạn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"jz7aylgw0x6-12","text":"I will not pop, spin, break for you.","type":"sentence","start_time":"00:00:43.260","end_time":"00:00:46.260","unapproved":1,"translation":{"text":"Tôi sẽ không bật, quay, phá vỡ cho bạn.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"jz7aylgw0x6-13","text":"I will not hate for you","type":"sentence","start_time":"00:00:46.260","end_time":"00:00:48.260","unapproved":1,"translation":{"text":"Tôi sẽ không ghét bạn","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"jz7aylgw0x6-14","text":"or even hate you.","type":"sentence","start_time":"00:00:48.260","end_time":"00:00:50.260","unapproved":1,"translation":{"text":"hoặc