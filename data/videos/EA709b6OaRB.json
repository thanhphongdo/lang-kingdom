{"data":[{"id":"EA709b6OaRB-1","text":"Hi. This is MonkeyEnglish.","type":"sentence","start_time":"00:00:13.100","end_time":"00:00:14.940","unapproved":1,"translation":{"text":"Chào. Đây là MonkeyEnglish.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-2","text":"I am Lukas.","type":"sentence","start_time":"00:00:15.580","end_time":"00:00:16.300","unapproved":1,"translation":{"text":"Tôi là Lukas.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-3","text":"I am a surgeon.","type":"sentence","start_time":"00:00:17.280","end_time":"00:00:18.980","unapproved":1,"translation":{"text":"Tôi là một bác sĩ phẫu thuật.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-4","text":"Hi. My name is Lukas.","type":"sentence","start_time":"00:00:32.920","end_time":"00:00:34.280","unapproved":1,"translation":{"text":"Chào. Tôi tên là Lukas","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-5","text":"I am a patient.","type":"sentence","start_time":"00:00:35.100","end_time":"00:00:36.420","unapproved":1,"translation":{"text":"Tôi là một bệnh nhân.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-6","text":"He is my patient.","type":"sentence","start_time":"00:00:38.340","end_time":"00:00:40.580","unapproved":1,"translation":{"text":"Anh ấy là bệnh nhân của tôi.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-7","text":"And we are going to make a surgery.","type":"sentence","start_time":"00:00:41.000","end_time":"00:00:44.520","unapproved":1,"translation":{"text":"Và chúng tôi sẽ làm một cuộc phẫu thuật.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-8","text":"This is an operating theatre.","type":"sentence","start_time":"00:00:47.080","end_time":"00:00:52.140","unapproved":1,"translation":{"text":"Đây là một nhà hát hoạt động.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-9","text":"This is a stretcher.","type":"sentence","start_time":"00:00:57.840","end_time":"00:00:59.700","unapproved":1,"translation":{"text":"Đây là một cáng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-10","text":"I am sitting on the stretcher.","type":"sentence","start_time":"00:00:59.960","end_time":"00:01:02.600","unapproved":1,"translation":{"text":"Tôi đang ngồi trên cáng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-11","text":"We are going to make a surgery.","type":"sentence","start_time":"00:01:09.940","end_time":"00:01:13.960","unapproved":1,"translation":{"text":"Chúng tôi sẽ làm một cuộc phẫu thuật.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-12","text":"This is a syringe.","type":"sentence","start_time":"00:01:19.660","end_time":"00:01:22.800","unapproved":1,"translation":{"text":"Đây là một ống tiêm.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-13","text":"And this is an injection.","type":"sentence","start_time":"00:01:28.280","end_time":"00:01:31.420","unapproved":1,"translation":{"text":"Và đây là một mũi tiêm.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-14","text":"These are gloves.","type":"sentence","start_time":"00:01:37.260","end_time":"00:01:39.780","unapproved":1,"translation":{"text":"Đây là găng tay.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-15","text":"I will need gloves.","type":"sentence","start_time":"00:01:42.800","end_time":"00:01:45.240","unapproved":1,"translation":{"text":"Tôi sẽ cần găng tay.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-16","text":"I am not a real doctor.","type":"sentence","start_time":"00:02:07.640","end_time":"00:02:10.940","unapproved":1,"translation":{"text":"Tôi không phải là một bác sĩ thực sự.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-17","text":"These are tweezers.","type":"sentence","start_time":"00:02:22.100","end_time":"00:02:25.380","unapproved":1,"translation":{"text":"Đây là nhíp.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-18","text":"These are needels.","type":"sentence","start_time":"00:02:37.280","end_time":"00:02:40.020","unapproved":1,"translation":{"text":"Đây là những thứ cần thiết.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-19","text":"A needle.","type":"sentence","start_time":"00:02:46.400","end_time":"00:02:48.500","unapproved":1,"translation":{"text":"Một cây kim.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-20","text":"and a thread.","type":"sentence","start_time":"00:02:49.960","end_time":"00:02:58.860","unapproved":1,"translation":{"text":"và một chủ đề.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-21","text":"We need antiseptics.","type":"sentence","start_time":"00:03:11.000","end_time":"00:03:14.220","unapproved":1,"translation":{"text":"Chúng ta cần thuốc sát trùng.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-22","text":"Lukas. Where is your injury?","type":"sentence","start_time":"00:03:20.440","end_time":"00:03:23.500","unapproved":1,"translation":{"text":"Lukas. Chấn thương của bạn ở đâu?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-23","text":"Easy. Calm down! Calm down!","type":"sentence","start_time":"00:03:27.020","end_time":"00:03:30.440","unapproved":1,"translation":{"text":"Dễ dàng. Bình tĩnh! Bình tĩnh!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-24","text":"This is a gauze.","type":"sentence","start_time":"00:03:43.260","end_time":"00:03:46.100","unapproved":1,"translation":{"text":"Đây là một miếng gạc.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-25","text":"I am cleaning the injury.","type":"sentence","start_time":"00:03:47.340","end_time":"00:03:51.260","unapproved":1,"translation":{"text":"Tôi đang làm sạch vết thương.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-26","text":"Fine. Are you ready?","type":"sentence","start_time":"00:03:52.420","end_time":"00:03:54.800","unapproved":1,"translation":{"text":"Khỏe. Bạn đã sẵn sàng chưa?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-27","text":"Local anaesthetics.","type":"sentence","start_time":"00:03:57.700","end_time":"00:04:00.540","unapproved":1,"translation":{"text":"Gây tê cục bộ.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-28","text":"It's difficult.","type":"sentence","start_time":"00:04:03.260","end_time":"00:04:05.380","unapproved":1,"translation":{"text":"Thật khó.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-29","text":"This is a scalpel.","type":"sentence","start_time":"00:04:23.160","end_time":"00:04:26.560","unapproved":1,"translation":{"text":"Đây là một con dao mổ.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-30","text":"Calm down! Calm down!","type":"sentence","start_time":"00:04:35.860","end_time":"00:04:37.380","unapproved":1,"translation":{"text":"Bình tĩnh! Bình tĩnh!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-31","text":"Keep it here!","type":"sentence","start_time":"00:04:41.440","end_time":"00:04:42.320","unapproved":1,"translation":{"text":"Giữ nó ở đây!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-32","text":"Fine.","type":"sentence","start_time":"00:04:44.880","end_time":"00:04:45.900","unapproved":1,"translation":{"text":"Khỏe.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-33","text":"I need a plaster.","type":"sentence","start_time":"00:04:46.020","end_time":"00:04:47.140","unapproved":1,"translation":{"text":"Tôi cần một thạch cao.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-34","text":"Fine.","type":"sentence","start_time":"00:04:47.400","end_time":"00:04:48.580","unapproved":1,"translation":{"text":"Khỏe.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-35","text":"A plaster.","type":"sentence","start_time":"00:04:50.400","end_time":"00:05:00.760","unapproved":1,"translation":{"text":"Một thạch cao.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-36","text":"This is an adhesive plaster.","type":"sentence","start_time":"00:05:04.200","end_time":"00:05:07.360","unapproved":1,"translation":{"text":"Đây là một thạch cao dính.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA709b6OaRB-