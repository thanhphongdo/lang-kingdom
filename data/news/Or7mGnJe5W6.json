rt_time":"00:00:27.827","end_time":"00:00:30.029","unapproved":1,"translation":{"text":"[Chúc mừng và vỗ tay]","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-15","text":"Ravishing sweater.","type":"sentence","start_time":"00:00:30.029","end_time":"00:00:32.365","unapproved":1,"translation":{"text":"Áo len đẹp.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-16","text":"Now, since there are five shows left,","type":"sentence","start_time":"00:00:32.365","end_time":"00:00:34.600","unapproved":1,"translation":{"text":"Bây giờ, vì có năm chương trình còn lại,","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-17","text":"let's open door number 5!","type":"sentence","start_time":"00:00:34.600","end_time":"00:00:36.836","unapproved":1,"translation":{"text":"Hãy mở cửa số 5!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-18","text":"Go for it. [ Drumroll ]","type":"sentence","start_time":"00:00:36.836","end_time":"00:00:37.971","unapproved":1,"translation":{"text":"Cứ liều thử đi. [ Trống cuộn ]","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-19","text":"Yeah! Whoo!","type":"sentence","start_time":"00:00:37.971","end_time":"00:00:39.939","unapproved":1,"translation":{"text":"Vâng! Ôi!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-20","text":"Look at that one.","type":"sentence","start_time":"00:00:39.939","end_time":"00:00:41.441","unapproved":1,"translation":{"text":"Nhìn vào cái đó","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-21","text":"[ Cheers and applause ]","type":"sentence","start_time":"00:00:41.441","end_time":"00:00:46.278","unapproved":1,"translation":{"text":"[Chúc mừng và vỗ tay]","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-22","text":"Not bad.","type":"sentence","start_time":"00:00:46.278","end_time":"00:00:47.947","unapproved":1,"translation":{"text":"Không tệ.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-23","text":"Santa's barn.","type":"sentence","start_time":"00:00:47.947","end_time":"00:00:50.483","unapproved":1,"translation":{"text":"Nhà kho của ông già Noel.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-24","text":"Turn it over.","type":"sentence","start_time":"00:00:50.483","end_time":"00:00:51.850","unapproved":1,"translation":{"text":"Lật lại","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-25","text":"Eight maids a milking.","type":"sentence","start_time":"00:00:51.850","end_time":"00:00:53.085","unapproved":1,"translation":{"text":"Tám người giúp việc một vắt sữa.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-26","text":"Eight maids a milking right there.","type":"sentence","start_time":"00:00:53.085","end_time":"00:00:54.587","unapproved":1,"translation":{"text":"Tám người giúp việc vắt sữa ngay tại đó.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-27","text":"[ Cheers and applause ]","type":"sentence","start_time":"00:00:54.587","end_time":"00:00:57.390","unapproved":1,"translation":{"text":"[Chúc mừng và vỗ tay]","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-28","text":"Now let's see who's going home with tonight's sweater!","type":"sentence","start_time":"00:00:57.390","end_time":"00:00:59.592","unapproved":1,"translation":{"text":"Bây giờ hãy xem ai về nhà với áo len tối nay!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-29","text":"Everyone look at your seat number!","type":"sentence","start_time":"00:00:59.592","end_time":"00:01:01.093","unapproved":1,"translation":{"text":"Mọi người nhìn vào số ghế của bạn!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-30","text":"If I call your number, I need you to jump up!","type":"sentence","start_time":"00:01:01.093","end_time":"00:01:03.028","unapproved":1,"translation":{"text":"Nếu tôi gọi số của bạn, tôi cần bạn nhảy lên!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-31","text":"Let me know where you are!","type":"sentence","start_time":"00:01:03.028","end_time":"00:01:04.097","unapproved":1,"translation":{"text":"Hãy cho tôi biết bạn đang ở đâu!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-32","text":"Quest, can I get a drumroll, please?","type":"sentence","start_time":"00:01:04.097","end_time":"00:01:05.865","unapproved":1,"translation":{"text":"Quest, làm ơn cho tôi xin một cái trống","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-33","text":"[ Drumroll ]","type":"sentence","start_time":"00:01:05.865","end_time":"00:01:06.899","unapproved":1,"translation":{"text":"[ Trống cuộn ]","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-34","text":"Who wants me to pick their number?","type":"sentence","start_time":"00:01:06.899","end_time":"00:01:08.401","unapproved":1,"translation":{"text":"Ai muốn tôi chọn số của họ?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-35","text":"[ Cheers and applause ]","type":"sentence","start_time":"00:01:08.401","end_time":"00:01:13.906","unapproved":1,"translation":{"text":"[Chúc mừng và vỗ tay]","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-36","text":"329!","type":"sentence","start_time":"00:01:13.906","end_time":"00:01:16.375","unapproved":1,"translation":{"text":"329!","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-37","text":"♪♪","type":"sentence","start_time":"00:01:16.375","end_time":"00:01:18.211","unapproved":1,"translation":{"text":"♪♪","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-38","text":"[ Cheers and applause ]","type":"sentence","start_time":"00:01:18.211","end_time":"00:01:20.313","unapproved":1,"translation":{"text":"[Chúc mừng và vỗ tay]","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-39","text":"♪♪","type":"sentence","start_time":"00:01:20.313","end_time":"00:01:30.222","unapproved":1,"translation":{"text":"♪♪","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-40","text":"How you doing, man?","type":"sentence","start_time":"00:01:30.222","end_time":"00:01:31.991","unapproved":1,"translation":{"text":"Cậu thế nào rồi anh bạn?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-41","text":"Wow.","type":"sentence","start_time":"00:01:31.991","end_time":"00:01:33.326","unapproved":1,"translation":{"text":"Ồ","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-42","text":"You really got a bad seat tonight.","type":"sentence","start_time":"00:01:33.326","end_time":"00:01:35.528","unapproved":1,"translation":{"text":"Bạn thực sự có một chỗ ngồi tồi tệ tối nay.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-43","text":"How are you, buddy? -I'm great.","type":"sentence","start_time":"00:01:35.528","end_time":"00:01:37.930","unapproved":1,"translation":{"text":"Cậu thế nào rồi anh bạn? -Tôi thật tuyệt.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-44","text":"-What's your name? -Drew.","type":"sentence","start_time":"00:01:37.930","end_time":"00:01:39.298","unapproved":1,"translation":{"text":"-Bạn tên là gì? -Đã vẽ.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-45","text":"-Drew, where are you from? -I'm from Philly.","type":"sentence","start_time":"00:01:39.298","end_time":"00:01:41.133","unapproved":1,"translation":{"text":"-Drew, bạn đến từ đâu? -Tôi đến từ Philly.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-46","text":"-You're from Philadelphia?","type":"sentence","start_time":"00:01:41.133","end_time":"00:01:42.435","unapproved":1,"translation":{"text":"-Bạn đến từ Philadelphia?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-47","text":"[ Cheers and applause ]","type":"sentence","start_time":"00:01:42.435","end_time":"00:01:45.538","unapproved":1,"translation":{"text":"[Chúc mừng và vỗ tay]","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-48","text":"You got that joke in the monologue, right?","type":"sentence","start_time":"00:01:45.538","end_time":"00:01:47.507","unapproved":1,"translation":{"text":"Bạn có trò đùa đó trong độc thoại, phải không?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-49","text":"-Oh, yeah. I know who the Roots are.","type":"sentence","start_time":"00:01:47.507","end_time":"00:01:49.209","unapproved":1,"translation":{"text":"-Oh, vâng. Tôi biết Rễ là ai.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-50","text":"-Just in general. Yeah, okay. Never mind.","type":"sentence","start_time":"00:01:49.209","end_time":"00:01:51.143","unapproved":1,"translation":{"text":"-Chỉ cần nói chung. Vâng, được thôi. Đừng bận tâm.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-51","text":"I know who they are, too.","type":"sentence","start_time":"00:01:51.143","end_time":"00:01:52.278","unapproved":1,"translation":{"text":"Tôi cũng biết họ là ai","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-52","text":"But it gets very cold in Philly.","type":"sentence","start_time":"00:01:52.278","end_time":"00:01:55.081","unapproved":1,"translation":{"text":"Nhưng nó rất lạnh ở Philly.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-53","text":"-Yes. -Hence the scarf?","type":"sentence","start_time":"00:01:55.081","end_time":"00:01:56.382","unapproved":1,"translation":{"text":"-Đúng. -Có khăn quàng cổ?","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-54","text":"-Yes, and this pea coat. -And this beautiful pea coat.","type":"sentence","start_time":"00:01:56.382","end_time":"00:01:58.384","unapproved":1,"translation":{"text":"-Có, và chiếc áo khoác đậu này. -Và chiếc áo khoác đậu xinh đẹp này.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Or7mGnJe5W6-55","text":"And three layers underneath. Would you like to try this on?","type":"sentence","start_time":"00:01:58.384","end_time":"00:02:00.753","unapproved":1,"translation":{"text":"Và ba lớp bên dưới. Bạn có muốn thử cái này không?","google_translate":1},"recordStat"