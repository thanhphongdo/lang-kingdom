:{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zM8PmKL-33","text":"just to get them wasted. -Keep on remembering stuff.","type":"sentence","start_time":"00:01:22.282","end_time":"00:01:23.650","unapproved":1,"translation":{"text":"chỉ để làm cho họ lãng phí. -Giữ nhớ công cụ.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zM8PmKL-34","text":"-Mess with them.","type":"sentence","start_time":"00:01:23.650","end_time":"00:01:24.750","unapproved":1,"translation":{"text":"-Mess với họ.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zM8PmKL-35","text":"-\"Remember when you got ham slapped?\"","type":"sentence","start_time":"00:01:24.750","end_time":"00:01:26.219","unapproved":1,"translation":{"text":"- \"Nhớ khi bạn bị ham tát?\"","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zM8PmKL-36","text":"[ Laughter ]","type":"sentence","start_time":"00:01:26.219","end_time":"00:01:28.387","unapproved":1,"translation":{"text":"[Cười]","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zM8PmKL-37","text":"-Ham slap! [ Thwack! ]","type":"sentence","start_time":"00:01:28.387","end_time":"00:01:30.423","unapproved":1,"translation":{"text":"-Hỗ tát! [Thwack! ]","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zM8PmKL-38","text":"[ Laughter ]","type":"sentence","start_time":"00:01:30.423","end_time":"00:01:34.660","unapproved":1,"translation":{"text":"[Cười]","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"4EA0zM8PmKL-39","text":"-Ham slap! [ Thwack! ]","type":"sentence","start_time":"00:01:34.660","end_time":"00:01:36.695","unapproved":1,"translation":{"text":"-Hỗ tát! [Thwack! ]","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","