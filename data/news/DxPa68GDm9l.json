approved":1,"translation":{"text":"[Cười]","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa68GDm9l-19","text":"-There you go. You got to --","type":"sentence","start_time":"00:00:42.742","end_time":"00:00:43.843 align:start size:94% position:6%","unapproved":1,"translation":{"text":"-Có bạn đi. Bạn có để --","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa68GDm9l-20","text":"He's that fast, huh? -I'm not gay.","type":"sentence","start_time":"00:00:43.843","end_time":"00:00:46.513 align:start size:94% position:6%","unapproved":1,"translation":{"text":"Anh ta nhanh quá hả? -Tôi không phải là gay.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa68GDm9l-21","text":"-No.","type":"sentence","start_time":"00:00:46.513","end_time":"00:00:47.947 align:start size:94% position:6% line:92%","unapproved":1,"translation":{"text":"-Không.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa68GDm9l-22","text":"-But I thought about it.","type":"sentence","start_time":"00:00:47.947","end_time":"00:00:49.149 size:88% position:44% line:92%","unapproved":1,"translation":{"text":"-Nhưng tôi nghĩ về nó.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa68GDm9l-23","text":"[ Laughter ]","type":"sentence","start_time":"00:00:49.149","end_time":"00:00:52.252 align:start size:94% position:6% line:92%","unapproved":1,"translation":{"text":"[Cười]","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"DxPa68GDm9l-24","text":"-It's Ryan Reynolds. -It's Ryan Reynolds.","type":"sentence","start_time":"00:00:52.252","end_time":"00:00:54.788 size:75% position:38%","unapproved":1,"translation":{"text":"-Đó là Ryan Reynold. -Đó là Ryan Reynold.","google_translate":1},"recordStat":{"voice_text":""