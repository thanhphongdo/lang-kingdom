ent":"0","translation":{"text":"Ngồi đi."},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"EA7m9b6g20R","dic_all_id":"7GRmPr2AeaW","text":"Stand by your desks.","type":"sentence","phonetic":"stænd baɪ jʊr ---","sound":"https://static.langkingdom.com/media/15/68b58aa20032f35948a03c351f123085.mp3","comment":"0","translation":{"text":"Hãy đứng cạnh bàn của các bạn."},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"APJ07bOLYmz","dic_all_id":"DxP06dnzA59","text":"Come to the front of the class.","type":"sentence","phonetic":"kʌm tuː ðə frʌnt əv ðə klæs","sound":"https://static.langkingdom.com/media/15/b5cdf2a2e3264288e33c80a24bfd1674.mp3","comment":"0","translation":{"text":"Hãy lên (đứng) trước cả lớp."},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Jzn5k86XJmD","dic_all_id":"9evmOgdnLmO","text":"Write on the board.","type":"sentence","phonetic":"raɪt ɑːn ðə bɔːrd","sound":"https://static.langkingdom.com/media/15/ff7c118dd18bf357e391224a792fef53.mp3","comment":"0","translation":{"text":"Hãy viết lên bảng."},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"kJG5lEPZ4md","dic_all_id":"op35KMzpV0D","text":"Erase the blackboard.","type":"sentence","phonetic":"ɪˈreɪz ðə ˈblæk.bɔːrd","sound":"https://static.langkingdom.com/media/15/f216e80ae1e7feca3021891ee172bb49.mp3","comment":"0","translation":{"text":"Hãy xóa bảng."},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"APJ07bObomz","dic_all_id":"DxP06dnqe59","text":"Please look at the blackboard.","type":"sentence","phonetic":"pliːz lʊk æt ðə ˈblæk.bɔːrd","sound":"https://static.langkingdom.com/media/15/369d30f63d72e9b01bf37d35bbf674e5.mp3","comment":"0","translation":{"text":"Xin hãy nhìn vào bảng đen.","google_translate":1},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"7GRmPrO14aW","dic_all_id":"Jznmk8vqM0D","text":"Pay attention, everybody.","type":"sentence","phonetic":"peɪ əˈten.ʃən ˈevrɪˌbɑːdɪ","sound":"https://static.langkingdom.com/media/15/381377bedc683f843487de1d8d5bcc21.mp3","comment":"0","translation":{"text":"Xin mọi người chú ý."},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"7Qn0w4oj3aP","dic_all_id":"vp25v4Bb0Bk","text":"Put your hands up.","type":"sentence","phonetic":"pʊt jʊr hændz ʌp","sound":"https://static.langkingdom.com/media/1/07ab535b31a473b94acddb424b8273bd.mp3","comment":"0","translation":{"text":"Hãy giơ tay lên nào."},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"J48apod4M0g","dic_all_id":"lG6mejkQB0e","text":"May I go to the restroom?","type":"sentence","phonetic":"meɪ aɪ ɡoʊ tuː ðə ˈrest.ruːm","sound":"https://static.langkingdom.com/media/8/ca19fcfefbc8b3af8fa8c0f60f85308e.mp3","comment":"0","translation":{"text":"Em xin phép đi vệ sinh!"},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"lOX5bKv8xmg","dic_all_id":"xn7mVwbB60J","text":"May I go out?","type":"sentence","phonetic":"meɪ aɪ ɡoʊ aʊt","sound":"https://static.langkingdom.com/media/8/8660352ef9019fdbe1af7b40b0b617ab.mp3","comment":"0","translation":{"text":"Em xin phép ra ngoài ạ?"},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"kp6a2nRgban","dic_all_id":"WLE5jzGqB5X","text":"May I go home a little early?","type":"sentence","phonetic":"meɪ aɪ ɡoʊ hoʊm ə ˈlɪt̬.əl ˈɝː.li","sound":"https://static.langkingdom.com/media/8/88f7a4a5e0081691f5b68fe48708f08f.mp3","comment":"0","translation":{"text":"Tôi có thể về nhà sớm một chút được không?"},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"3z70rkrO7av","dic_all_id":"9ev5OgdzD5O","text":"Get into a queue.","type":"sentence","phonetic":"ɡet ˈɪn.tuː ə kjuː","sound":"https://static.langkingdom.com/media/8/ca82503f61af0ac435250c65dccf998b.mp3","comment":"0","translation":{"text":"Hãy tập hợp lại thành một hàng dọc."},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"VYP5YOrWz0M","dic_all_id":"op3aKMzjMmD","text":"Form a queue and wait for the bell.","type":"sentence","phonetic":"fɔːrm ə kjuː ænd weɪt fɔːr ðə bel","sound":"https://static.langkingdom.com/media/8/82838893d66e54151bbddaf1b8aef96f.mp3","comment":"0","translation":{"text":"Hãy xếp hàng và chờ tiếng chuông."},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"LwW0drRLL5D","dic_all_id":"GErmB2V7Vm3","text":"Everybody outside!","type":"sentence","phonetic":"ˈevrɪˌbɑːdɪ ˌaʊtˈsaɪd","sound":"https://static.langkingdom.com/media/8/465507578a51a62c17110997dce46481.mp3","comment":"0","translation":{"text":"Mọi người (nhanh chóng) ra ngoài!"},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"7R6aZZ2DZaK","dic_all_id":"ZrP04g1qdmQ","text":"All of you, get outside now!","type":"sentence","phonetic":"ɑːl əv juː ɡet ˌaʊtˈsaɪd naʊ","sound":"https://static.langkingdom.com/media/8/bdbf38d07eabfa52502e7d12e8cc336e.mp3","comment":"0","translation":{"text":"Tất cả các em, hãy đi ra ngoài ngay!"},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"Vj658gXzOaA","dic_all_id":"dgp01Bkqda8","text":"Hurry up and get out.","type":"sentence","phonetic":"ˈhʌrɪ ʌp ænd ɡet aʊt","sound":"https://static.langkingdom.com/media/8/458bbd8c3e22863fd261fbc034cd62c7.mp3","comment":"0","translation":{"text":"Hãy nhanh chóng đi ra ngoài."},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"QLv0ooZoK0K","dic_all_id":"b2J0RZwyQ0d","text":"Try not to make any noise as you leave.","type":"sentence","phonetic":"traɪ nɑːt tuː meɪk ˈen.i nɔɪz æz juː liːv","sound":"https://static.langkingdom.com/media/8/841a59333dd016c9a5a437a6289f3120.mp3","comment":"0","translation":{"text":"Hãy cố gắng đừng gây ồn khi các em ra về/rời khỏi nhé."},"recordStat":{"voice_text":"","voice_audio":"","right":0,"wrong":0}},{"id":"7GR0PrOr45W","dic_all_id":"qPJaLxnykaK","text":"Be quiet as you leave.","